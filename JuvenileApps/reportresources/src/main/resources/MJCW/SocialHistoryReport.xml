<?xml version="1.0" encoding="US-ASCII"?>
<document size="A4" margin-left="25" margin-right="25" margin-top="25" margin-bottom="25">

	<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="false"/>
	<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="Social History Report"/>

	<module>
		<font-def name="arial-9-bold-u" family="arial" style="bold|underline" size="9"/>
		<font-def name="arial-10-bold-u" family="arial" style="bold|underline" size="10"/>
		<font-def name="arial-10-bold" family="arial" style="bold" size="10"/>
		<font-def name="arial-10" family="arial" size="10"/>
		<font-def name="arial-11-bold" family="arial" style="bold" size="11"/>
		<font-def name="arial-11" family="arial" size="11"/>
		<font-def name="arial-12" family="arial" style="normal" size="12"/>
		<font-def name="arial-12-bold" family="arial" style="bold" size="12"/>
		<font-def name="arial-12-bold-u" family="arial" style="bold|underline" size="12"/>
		<font-def name="arial-14" family="arial" style="normal" size="14"/>
		
		<font-def name="title" family="Helvetica" style="bold" size="12"/>
		<font-def name="header" family="Helvetica" style="bold|underline" size="12"/>
		<font-def name="label" family="Helvetica" style="bold" size="11"/>
		<font-def name="text" family="arial" style="normal" size="8"/>
		<font-def name="text-large" family="Helvetica" style="normal" size="12"/>
		<font-def name="tableheader" family="Helvetica" style="bold|underline" size="10"/>
		<font-def name="columnheader" family="Helvetica" style="bold|underline" size="10"/>

		<custom-tag alias="shr-title" tag="paragraph">
			<attribute name="font" value="title"/>
		</custom-tag>
		
		<custom-tag alias="shr-header" tag="paragraph">
			<attribute name="font" value="header"/>
		</custom-tag>
		
		<custom-tag alias="shr-label" tag="phrase">
			<attribute name="font" value="label"/>
		</custom-tag>
		
		<custom-tag alias="shr-text" tag="paragraph">
			<attribute name="font" value="text"/>
		</custom-tag>
		
		<custom-tag alias="shr-tableheader" tag="phrase">
			<attribute name="font" value="tableheader"/>
		</custom-tag>
		
		<custom-tag alias="column-header" tag="paragraph">
			<attribute name="font" value="columnheader"/>
		</custom-tag>
	</module>
	
	<font-def name="helvetica" family="Helvetica" style="normal" size="12"/>
	<font-def name="geneva" family="georgia" style="normal" size="12"/>

	<footer padding-top="10">
		<font size="10" color="black">
		<footer-part padding-top="15" halign="left" width="500">
		<table columns="1" width="100" halign="center">
			<cell halign="center">
				Page : ${pageNumber , nointrprt} of ${pageCount , nointrprt}
			</cell>
			<cell halign="center"><br/></cell>
		</table>
		</footer-part>
		</font>
	</footer>
	
	_if_(${generic == false})
		<!--
	        COVER PAGE
		-->
		<table columns="1" border-style="none" border-width="0" widths="1" padding="2">
      <cell halign="left" valign="top" leading="1" padding-top="0" line-spacing="1">
        <paragraph>
          <image source="MJCW/HarrisCountySeal.jpg" width="75" height="75" scale-to-fit="true"/>
        </paragraph>
      </cell>
		</table>
		
		<paragraph font="arial-12-bold" halign="center" spacing-after="20">
			IN THE ${presentOffense.courtName}<br/>
			HARRIS COUNTY<br/>
			STATE OF TEXAS
		</paragraph>
		
		<paragraph font="arial-12-bold" halign="left" spacing-after="10">
			IN THE MATTER OF: ${juvenileName}<br/>
			NO: ${presentOffense.petitionNumber}<br/>
			<br/>
		</paragraph>
			
		<paragraph font="arial-12-bold" halign="left" spacing-after="10">
			<u>Exhibit B</u>
		</paragraph>
			
		<paragraph font="arial-12-bold" halign="left" spacing-after="10">
			<i>The court finds that the following efforts were considered in determining whether 
			reasonable efforts have been made to prevent or eliminate the need for the child 
			to be removed from (his or her) home:</i>
		</paragraph>
		
		<table columns="2" widths="5,95" border-style="none" font="arial-12-bold">
			<table-row padding-bottom="10">
				<cell>_____</cell>
				<cell>The child was previously placed on probation by the Court.</cell>
			</table-row>
			<table-row padding-bottom="10">
				<cell>_____</cell>
				<cell>The child and/or family was previously referred to the following community, court, or educational programs: (list programs)</cell>
			</table-row>
			<table-row padding-bottom="10">
				<cell>_____</cell>
				<cell>The child and/or family was previously referred to the following counseling or psychological services: (list programs)</cell>
			</table-row>
			<table-row padding-bottom="10">
				<cell>_____</cell>
				<cell>The child and/or family is receiving or has previously received services from TDPRS or MHMRA.</cell>
			</table-row>
			<table-row padding-bottom="10">
				<cell>_____</cell>
				<cell>The nature of the offense and/or circumstances in the child?s home necessitates removal of the child from the home.</cell>
			</table-row>
			<table-row font="arial-12" padding-bottom="10">
				<cell>_____</cell>
				<cell><i>(List any other specific circumstances not addressed above)</i></cell>
			</table-row>
		</table>
	_endif_
	
	<!--
        DETAIL PAGES
	-->
	<new-page size="A4" margin-left="25" margin-right="25" margin-top="25" margin-bottom="25">
	</new-page>
	
    <table columns="2" border-style="none" border-width="0" widths="40,60" padding="0" spacing-after="10">
	    <cell halign="left" valign="top" leading="1" padding-top="0" line-spacing="1">
	      <paragraph>
		      <image source="MJCW/TexasJuvenileProbationCommisionSeal.jpg" width="75" height="75" scale-to-fit="true"/>
	      </paragraph>
	    </cell>
			<cell font="arial-14" valign="bottom" halign="left"><b>JUVENILE PROBATION REPORT</b></cell>
		</table>
	
	<table width="100" columns="1" halign="left" spacing-after="10">
		<cell>
			<shr-label trim-body="true">Name:</shr-label> ${juvenileName}<br/>
		</cell>
		<cell>
			<shr-label trim-body="true">DOB:</shr-label> ${dateOfBirth}<br/>
		</cell>
		<cell>
			<shr-label trim-body="true">Petition #:</shr-label> ${allPetitionNum}<br/>
		</cell>
		<cell>
			<shr-label trim-body="true">Juvenile #:</shr-label> ${juvenileNumber}<br/>
		</cell>
		
		_if_(${generic == false})
			<cell>
				<shr-label trim-body="true">Court Date:</shr-label> ${presentOffense.courtDate}<br/>
			</cell>
			<cell>
				<shr-label trim-body="true">Date Interviewed:</shr-label><br/>
			</cell>
			<cell>
				<shr-label trim-body="true">Probation Officer</shr-label> ${probationOfficer}<br/>
			</cell>
		_endif_
	</table>
	<br/>
	
	<!--
        PRESENT OFFENSE
	-->
	<shr-header>PRESENT OFFENSE</shr-header><br/>
	<table width="100" columns="2" widths="17,83">
		_foreach_(offense ${presentOffenses} 'even' 'odd')
			<if cond="${offense.included==true}"> 
				<table-row halign="left">
					<cell font="arial-10-bold" no-wrap="true">Nature of Offense:</cell>
					<cell halign="left"><shr-text>${offense.presentOffense}</shr-text></cell>
				</table-row>
				
				_if_ (${offense.adultCoActors} notEmpty)
				<table-row padding-bottom="5">
					<cell fill-row="true">
						<paragraph font="arial-11-bold">Adult Co-Actors:</paragraph>
						<table width="100" columns="2" padding="3">
							<table-header font="arial-9-bold-u">
								<cell>Name</cell>
								<cell>Age</cell>
							</table-header>
							_foreach_(coactor ${offense.adultCoActors} 'even' 'odd')
							<table-row>
								<cell halign="left"><shr-text>${coactor.name}</shr-text></cell>
								<cell halign="left"><shr-text>${coactor.age}</shr-text></cell>
							</table-row>
							_endfor_
						</table>
					</cell>
				</table-row>
				_endif_
			</if>
		_endfor_
		_if_(${generic == false})
			_if_(${latestPofActivity isDefined})
				<table-row halign="left">
					<cell halign="left" colspan='2'><shr-text>${latestPofActivity.comments}</shr-text></cell>
				</table-row>
			_endif_
		_endif_
	</table>
	<br/>
	
	_if_(${generic == false})
		<!--
	        SIGNIFICANT INFORMATION
		-->
		_if_(${latestEvsActivity isDefined})
			<shr-header>SIGNIFICANT INFORMATION</shr-header>
			<br />
			<table width="83" columns="1" > <!-- 80,83 "text-large" halign="left" <shr-text> -->
				<table-row>
					<cell halign="left" valign="top">
						<paragraph font="arial-10">${latestEvsActivity.comments}</paragraph>
					</cell>
				</table-row>
			</table>
			<br/>
		_endif_
	_endif_

	<!--
        PREVIOUS REFERRALS
	-->
	<shr-header>PREVIOUS REFERRALS</shr-header>
    <br/>
	<table width="100" columns="5" padding="3">
		<table-row halign="left">
		<cell><column-header>Referral Date</column-header></cell>
			<cell><column-header>Offense/Petition Allegation</column-header></cell>
			<cell><column-header>Offense/Petition Date </column-header></cell>
			<cell><column-header>Intake Decision /Court Disposition</column-header></cell>
			<cell><column-header>Intake/Court Date</column-header></cell>
		</table-row>
		
   	_foreach_(refW ${referralDetails} 'even' 'odd')
			_if_(${refW.petitionAvailable})
					<table-row>
	    			<cell halign="left" valign="top"><shr-text>${refW.referralData.referralDate}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${refW.petitionAllegation}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${refW.petitionFileDate}</shr-text></cell>
	    			<cell halign="left" valign="top"><shr-text>${refW.courtDisposition}</shr-text></cell>
	    			<cell halign="left" valign="top"><shr-text>${refW.referralData.courtDate}</shr-text></cell>
	    		</table-row>
		  _else_
	    	_foreach_(offenseIn ${refW.offenses} 'even' 'odd')
		    	<table-row>
	    			<cell halign="left" valign="top"><shr-text>${refW.referralData.referralDate}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${offenseIn.petitionAllegation}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${offenseIn.offenseDate}</shr-text></cell>
	    			<cell halign="left" valign="top"><shr-text>${refW.referralData.intakeDecision}</shr-text></cell>
	    			<cell halign="left" valign="top"><shr-text>${refW.intakeDate}</shr-text></cell>
	    		</table-row>
		    _endfor_
		  _endif_
		_endfor_	
	</table>
	<br/>
	<!--
        END PREVIOUS REFERRALS
	-->
	
	<!--
        FAMILY EMPLOYMENT/INCOME
	-->
	<shr-header>FAMILY</shr-header>
	<br/>
	<table width="100" columns="2" padding="1">
		_foreach_(info ${familyFinancialHistory} 'even' 'odd')
 			<table-row halign="left">
				<cell halign="left" valign="top">
					<shr-label>Family Personal Data</shr-label>
				</cell>
				<cell halign="left" valign="top">				
				</cell>
			</table-row>
			<table-row halign="left">
				<cell><column-header>Guardian</column-header></cell>
				<cell><column-header>Relationship</column-header></cell>
			</table-row>
			<table-row>
				<cell halign="left" valign="top"><shr-text>${info.guardian}</shr-text></cell>
				<cell halign="left" valign="top"><shr-text>${info.relationship}</shr-text></cell>
			</table-row>
			<table-row halign="left">
				<cell fill-row="true">
					<table columns="2" widths="20,80">
						<cell padding-left="10">Address:</cell>
						<cell><shr-text>${info.address}</shr-text></cell>
					</table>
				</cell>
			</table-row>
			<table-row halign="left">
				<cell fill-row="true">
					<table columns="2" widths="20,80">
						<cell padding-left="10">Phone#:</cell>
						<cell><shr-text>${info.formattedPhone}</shr-text></cell>
					</table>
				</cell>
			</table-row>
			<table-row><cell><br/></cell></table-row>
			_if_(${allGuardiansInHome == false})
	 			<table-row halign="left">
					<cell halign="left" valign="top">
						<shr-label>Family Employment / Income</shr-label> 
					</cell>
					<cell halign="left" valign="top">				
					</cell>
				</table-row>
				<table-row halign="left">
					<cell halign="left" valign="top">
						<shr-label>Total Gross Family Income: </shr-label><shr-text>$<space/>${info.totalGross} </shr-text>
					</cell>
					<cell halign="left" valign="top">
						<shr-label>Number of People in Household: </shr-label><shr-text><space/>${info.numberLivingInHome}</shr-text>
					</cell>
				</table-row>
				<table-row>
					<cell fill-row="true">
						<table columns="4" widths="20,20,30,26">
							<table-header font="tableheader">
								<cell>Employed by</cell>
								<cell>Status</cell>
								<cell>Job Description</cell> 
								<cell>Entry Date</cell>
							</table-header>
							_foreach_(employment ${info.employmentHistory} 'even' 'odd')
								<table-row font="text">	
									<cell>${employment.placeEmployed}</cell>
									<cell>${employment.employmentStatus}</cell>
									<cell>${employment.jobDescription}</cell>
									<cell>${employment.entryDate}</cell>
								</table-row>
							_endfor_
						</table>
					</cell>
				</table-row>
			_endif_		
			<table-row><cell><br/></cell></table-row>
		_endfor_
		 _if_(${allGuardiansInHome})
		 	<table-row halign="left">
				<cell halign="left" valign="top">
					<shr-label>Family Employment / Income</shr-label>
				</cell>
				<cell halign="left" valign="top">					
				</cell>
			</table-row>
			<table-row halign="left">
				<cell halign="left" valign="top">
					<shr-label>Total Gross Family Income: </shr-label><shr-text>$<space/>${totalGrossFamilyIncome} </shr-text>
				</cell>
				<cell halign="left" valign="top">
					<shr-label>Number of People in Household: </shr-label><shr-text><space/>${numberInHousehold}</shr-text>
				</cell>
			</table-row>
			<table-row>
				<cell fill-row="true">
					<table columns="4" widths="20,20,30,26">
						<table-header font="tableheader">
							<cell>Employed by</cell>
							<cell>Status</cell>
							<cell>Job Description</cell> 
							<cell>Entry Date</cell>
						</table-header>
						_foreach_(info ${familyFinancialHistory} 'even' 'odd')
							_foreach_(employment ${info.employmentHistory} 'even' 'odd')
								<table-row font="text">	
									<cell>${employment.placeEmployed}</cell>
									<cell>${employment.employmentStatus}</cell>
									<cell>${employment.jobDescription}</cell>
									<cell>${employment.entryDate}</cell>
								</table-row>
							_endfor_
						_endfor_
					</table>
				</cell>
			</table-row>
		_endif_		
	</table>
	<br/>
	
	_if_(${generic == false})
		<!--
	        THE CHILD
		-->
	    _if_ (${substanceAbuseInformation} notEmpty)
			<shr-header>THE CHILD</shr-header>
		
			<shr-label>Drug and Alcohol Usage</shr-label><br/>
			<table width="100" columns="6" padding="3">
				<table-row halign="left">
					<cell><column-header>Onset Age</column-header></cell>
					<cell><column-header>Drug Type</column-header></cell>
					<cell><column-header>Frequency</column-header></cell>
					<cell><column-header>Location of Use</column-header></cell>
					<cell><column-header>Amount Spent</column-header></cell>
					<cell><column-header>Degree</column-header></cell>
				</table-row>
				_foreach_(info ${substanceAbuseInformation} 'even' 'odd')
					_if_( ${info.included} )
					<table-row>
						<cell halign="left" valign="top"><shr-text>${info.onsetAge}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.drugType}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.frequency}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.locationOfUse}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.amountSpent}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.degree}</shr-text></cell>
					</table-row>
					_endif_
				_endfor_
			</table>
			<br/>
		_endif_
		
		_if_ (${substanceAbuseTraits} notEmpty)
			<shr-label>Substance Abuse:</shr-label><br/>
			<table width="100" columns="2" padding="3">
				<table-row halign="left">
					<cell><column-header>Description</column-header></cell>
					<cell><column-header>Status</column-header></cell>
				</table-row>
				_foreach_(trait ${substanceAbuseTraits} 'even' 'odd')
					_if_( ${trait.included} )
						<table-row>
							<cell halign="left" valign="top"><shr-text>${trait.traitTypeDescription}</shr-text></cell>
							<cell halign="left" valign="top"><shr-text>${trait.status}</shr-text></cell>
						</table-row>
					_endif_
				_endfor_
			</table>
			<br/>
		_endif_
		
		_if_ (${gangTraits} notEmpty)
			<shr-label>Gang Activity:</shr-label><br/>
			<table width="100" columns="2" padding="3">
				<table-row halign="left">
					<cell><column-header>Description</column-header></cell>
					<cell><column-header>Status</column-header></cell>
				</table-row>
				_foreach_(trait ${gangTraits} 'even' 'odd')
					_if_( ${trait.included} )
						<table-row>
							<cell halign="left" valign="top"><shr-text>${trait.traitTypeDescription}</shr-text></cell>
							<cell halign="left" valign="top"><shr-text>${trait.status}</shr-text></cell>
						</table-row>
					_endif_
				_endfor_
			</table>
			<br/>
		_endif_
			
		_if_ (${strengthTraits} notEmpty)
			<shr-label>Strengths:</shr-label><br/>
			<table width="100" columns="2" padding="3">
				<table-row halign="left">
					<cell><column-header>Description</column-header></cell>
					<cell><column-header>Status</column-header></cell>
				</table-row>
				_foreach_(trait ${strengthTraits} 'even' 'odd')
					_if_( ${trait.included} )
						<table-row>
							<cell halign="left" valign="top"><shr-text>${trait.traitTypeDescription}</shr-text></cell>
							<cell halign="left" valign="top"><shr-text>${trait.status}</shr-text></cell>
						</table-row>
					_endif_
				_endfor_
			</table>
			<br/>
		_endif_
		
		_if_ (${juvenileFacilityStayRecord} notEmpty)
			<shr-label>Facility History:</shr-label><br/>
			<table width="100" columns="4" padding="3" widths="30,15,40,15">
				<table-row halign="left">
					<cell><column-header>Name</column-header></cell>
					<cell><column-header>Admit Date</column-header></cell>
					<cell><column-header>Admit Reason</column-header></cell>
					<cell><column-header>Release Date</column-header></cell>
				</table-row>
				_foreach_(stayRecord ${juvenileFacilityStayRecord} 'even' 'odd')					
					<table-row>
						<cell halign="left" valign="top"><shr-text>${stayRecord.facilityName}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${stayRecord.admitDate}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${stayRecord.admitReasonDesc}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${stayRecord.releaseDate}</shr-text></cell>
					</table-row>
					
				_endfor_
			</table>
			<br/>
		_endif_

		_if_ (${employmentHistory} notEmpty)
			<shr-label>Youth Employment History:</shr-label><br/>
			<table width="100" columns="6" padding="3">
				<table-row halign="left">
					<cell><column-header>Employed by</column-header></cell>
					<cell><column-header>Status</column-header></cell>
					<cell><column-header>Supervisor</column-header></cell>
					<cell><column-header>Job Description</column-header></cell>
					<cell><column-header>Hours</column-header></cell>
					<cell><column-header>Entry Date</column-header></cell>
				</table-row>
				_foreach_(info ${employmentHistory} 'even' 'odd')
					_if_( ${info.included} )
					<table-row>
						<cell halign="left" valign="top"><shr-text>${info.placeEmployed}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.employmentStatus}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.supervisorName}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.jobDescription}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.workHours}</shr-text></cell>
						<cell halign="left" valign="top"><shr-text>${info.entryDate}</shr-text></cell>
					</table-row>
					_endif_
				_endfor_
			</table>
			<br/>
		_endif_
	_endif_
	
	<!--
        SCHOOL
	-->
	_if_ (${educationalHistory} notEmpty)
		<shr-header>SCHOOL</shr-header>
		<shr-label>Academic Performance:</shr-label><br/>
		<table width="100" columns="7" widths="30,10,14,7,12,15,12" padding="3">
			<table-header halign="left">
				<cell><column-header>Name</column-header></cell>
				<cell><column-header>Grade</column-header></cell>
				<cell><column-header>Appropriate Level</column-header></cell>
				<cell><column-header>GPA</column-header></cell>
				<cell><column-header>Last Attended</column-header></cell>
				<cell><column-header>Enrollment Status</column-header></cell>
				<cell><column-header>Verified</column-header></cell>
			</table-header>
			_foreach_(info ${educationalHistory} 'even' 'odd')
				_if_( ${info.included} )
				<table-row>
					<cell halign="left" valign="top"><shr-text>${info.district}</shr-text></cell>
