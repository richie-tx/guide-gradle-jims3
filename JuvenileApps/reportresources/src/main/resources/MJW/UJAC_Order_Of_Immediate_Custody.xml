<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="50" margin-bottom="60">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="true"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="DTA Warrant"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="arial_10" family="arial" style="normal" size="10"/>
	<font-def name="times" family="Times" style="normal" size="14"/>
	<font-def name="times_12" family="Times" style="normal" size="12"/>

	<header>
			<font name="times_12">
			<table columns="3" widths="30,35,30" spacing-before="30" border-style="none" keep-together="true" halign="center">
				<table-row halign="center">
					<cell halign="center" colspan="3"><font name="times"><b>Cause: ${petitionNum}</b></font></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="right"><b>The State Of Texas</b></cell>
					<cell halign="center" ><b>&#167;</b></cell>
					<cell halign="left"><b>${courtId} DISTRICT COURT</b></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="right"><b>County Of Harris</b></cell>
					<cell halign="center"><b>&#167;</b></cell>
					<cell halign="left"><b>Harris County, Texas</b></cell>
				</table-row>
				<table-row halign="center" padding="10">
					<cell halign="center" colspan="3"><b><u>ORDER OF IMMEDIATE CUSTODY</u></b></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="center" colspan="3"><b><u>Warrant Number: ${warrantNum}</u></b></cell>
				</table-row>
			</table>	
			</font>
	</header>
	
	<footer padding-top="0">
		<footer-part padding-top="0" halign="center" width="500">
			<table columns="1" border-style="none">
				<table-row>
					<font name="arial_10">
						<cell valign="top" padding-top="15" halign="center">****NOT VALID WITHOUT COURT SEAL****</cell>
					</font>
				</table-row>
	     	</table>
		</footer-part>
	</footer>	
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="20">
		<font name="times_12">IN THE MATTER OF ${firstName} ${middleName} ${lastName} ${suffix}, Juvenile Respondent
		</font>	
	</paragraph>
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="10">
		<font name="times_12">In the ${courtId} District Court of Harris County, Texas on this the <u>${fileStampDate}</u> it having been made known to the Court that the Juvenile Respondent, ${firstName} ${middleName} ${lastName} ${suffix}, a child over the age of 10 years and under the age of 18 years is charged on a petition filed by the District Attorney of Harris County, Texas with having engaged in delinquent conduct/conduct indicating a need for supervision, and that ${warrantOriginatorJudge} is seeking the detention of said child. 
		</font>	
	</paragraph>
	
	<if cond="${cautionsSelected != null && cautionsSelected != ""}">
		<paragraph halign="left" line-spacing="1.25" spacing-before="10">
			<font name="times_12">FURTHER, it appears that said child ${cautionsSelected} ${cautionComments}
			</font>
		</paragraph>
	</if>
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="10">
		<font name="times_12">IT IS THEREFORE ORDERED that any probation officer or peace officer take the said Juvenile Respondent and place said child in the custody and control of the Superintendent of the Harris County Juvenile Detention Home, Houston, Harris County, Texas, who shall provide maintenance, medical examination, emergency medical care and/or treatment as many be necessary for the health and welfare of said child, until final hearing can be had on the case or pending further orders of the Court. 
		</font>	
	</paragraph>
	
	<table columns="1" width="92" halign="left" spacing-before="10">
		<table-row>
			<cell halign="left" padding-top="10">
				<font name="times_12">Given under my hand on this__________day of___________________, __________.</font>
			</cell>
		</table-row>
	</table>
	
		<table columns="1" width="40" halign="right" spacing-before="50">
		<table-row>
			<cell halign="right">
				<font name="times_12">_____________________________</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right" padding-left="10">
				<font name="times_12">Presiding Judge
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right">
				<font name="times_12"> ${courtId}
				</font>
			</cell>
		</table-row>
	</table>					
					
</document>