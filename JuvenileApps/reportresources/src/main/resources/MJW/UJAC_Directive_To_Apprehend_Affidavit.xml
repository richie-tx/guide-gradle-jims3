<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="50" margin-bottom="60">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="true"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="DTA Warrant"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>
<module>
<custom-tag alias="strong" tag="b">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="em" tag="i">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="ul" tag="list">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="ol" tag="ul">
<attribute name="body" value="${body}"/>
<attribute name="numbered" value="true"/>
</custom-tag>
 
<custom-tag alias="li" tag="list-item">
<attribute name="body" value="${body}"/>
</custom-tag>

</module>

	<font-def name="helvetica" family="Helvetica" style="normal" size="12"/>
	<font-def name="geneva" family="georgia" style="normal" size="12"/>
	<font-def name="arial" family="arial" style="normal" size="12"/>
	<font-def name="arial_10" family="arial" style="normal" size="10"/>
	<font-def name="arial_8" family="arial" style="normal" size="8"/>
	<font-def name="times" family="Times" style="normal" size="12"/>
	<font-def name="times_10" family="Times" style="normal" size="10"/>

	<header>
			<font name="times" size="12" style="normal">
			<table columns="3" widths="30,35,30" spacing-before="30" border-style="none" keep-together="true" halign="center">
				<table-row halign="center">
					<cell halign="right"><b>The State Of Texas</b></cell>
					<cell halign="center" ><b>&#167;</b></cell>
					<cell halign="left"><b>Affidavit</b></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="right"><b></b></cell>
					<cell halign="center"><b>&#167;</b></cell>
					<cell halign="left"><b>For</b></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="right"><b></b></cell>
					<cell halign="center"><b>&#167;</b></cell>
					<cell halign="left"><b>Directive To Apprehend</b></cell>
				</table-row>
				<table-row halign="center">
					<cell halign="right"><b>County Of Harris</b></cell>
					<cell halign="center"><b>&#167;</b></cell>
					<cell halign="left"><b>Warrant Number: ${warrantNum}</b></cell>
				</table-row>
			</table>	
			</font>
	</header>
	
	<footer padding-top="0">
		<footer-part padding-top="0" halign="center" width="500">
			<table columns="1" border-style="none">
				<table-row>
					<font name="arial_10" size="10" style="normal">
						<cell valign="top" padding-top="15" halign="center">****NOT VALID WITHOUT COURT SEAL****</cell>
					</font>
				</table-row>
	     	</table>
		</footer-part>
	</footer>	
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="20" spacing-after="10">
		<font name="arial_10" size="10" style="normal">I, the undersigned affiant, am employed as a peace officer with the ${officerAgencyName} in Harris County, Texas.  I believe and have reason to believe that ${firstName} ${middleName} ${lastName} ${suffix}, hereinafter the Respondent, engaged in delinquent conduct by committing the ${offenseLevel} ${offenseDegree} offense of ${chargeDescription} in Harris County, Texas on or about ${offenseDateAsString}.  Affiant's belief is based on the following:
		</font>	
	</paragraph>
	
	<table width="100" widths="100" padding-bottom="6" spacing-before="10">
		<font name="arial_10">
			<table-row>
				<cell halign="left" valign="top" colspan="1" padding-left="10"> ${affidavitStatement}</cell>
			</table-row>
		</font>
	</table>
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="10" spacing-after="10">
		<font name="arial_10" size="10" style="normal">Affiant determined the Respondent was ${juvenileAge} years of age at the time of the offense by: 
		</font>	
	</paragraph>
	
	<table width="100" widths="100" padding-bottom="6" spacing-before="10">
		<font name="arial_10">
			<table-row>
				<cell halign="left" valign="top" colspan="1" padding-left="10"> ${ageVerification}</cell>
			</table-row>
		</font>
	</table>
	
	<paragraph halign="left" line-spacing="1.25" spacing-before="10" spacing-after="10">
		<font name="arial_10" size="10" style="normal">WHEREFORE, PREMISES CONSIDERED, affiant respectfully requests that a Directive to Apprehend issue for ${firstName} ${middleName} ${lastName} ${suffix} and that they be placed in the custody of the Chief Juvenile Probation Officer at the Juvenile Detention Center in Harris County, Texas. 
		</font>	
	</paragraph>
	
	<table columns="1" width="40" halign="right" spacing-before="50">
		<table-row>
			<cell halign="right">
				<font name="arial_10">_____________________________</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="center">
				<font name="arial_10">Affiant
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right">
				<font name="arial_10">_____________________________</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="center" padding-left="5">
				<font name="arial_10">PRINT NAME
				</font>
			</cell>
		</table-row>
	</table>
	
	<table columns="1" width="92" halign="left" spacing-before="50">
		<table-row>
			<cell halign="left" padding-top="10">
				<font name="arial_10">Sworn to and subscribed before me on the ________ day of __________________, ________.</font>
			</cell>
		</table-row>
	</table>
	
		<table columns="1" width="40" halign="right" spacing-before="50">
		<table-row>
			<cell halign="right">
				<font name="arial_10">_____________________________</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right" padding-left="10">
				<font name="arial_10">Presiding Judge
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right">
				<font name="arial_10">_____ District Court
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="right" padding-left="10">
				<font name="arial_10">Harris County, Texas 			
				</font>
			</cell>
		</table-row>
	</table>					
					
</document>