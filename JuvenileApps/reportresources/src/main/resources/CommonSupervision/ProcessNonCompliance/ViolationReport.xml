								<if cond="${nextOfficeVisitDate != null && nextOfficeVisitDate != ""}"><u>${nextOfficeVisitDate format "MM/dd/yyyy"}</u></if>
								<else>__________________</else><space/>@<space/><if cond="${nextOfficeVisitTime != null && nextOfficeVisitTime != ""}"><u>${nextOfficeVisitTime format "hh:mm a"}</u></if>
								<else>_______________</else>
							</cell>	
						</table-row>
					</table>
				</cell>
			</table-row>
		</font>
	</table>
	
	<table columns="1" border-style="none" keep-together="true" line-spacing="1.25" padding="0">
