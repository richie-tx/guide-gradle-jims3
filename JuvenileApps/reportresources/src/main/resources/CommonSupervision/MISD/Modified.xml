<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="50">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="false"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="arial" family="arial" style="normal" size="12"/>
	<font-def name="arial_10" family="arial" style="normal" size="10"/>
		
	<header>
		<header-part halign="left"  width="510">
				<table width="510" columns="2">
					<table-row halign="center">
						<font name="arial">
							<cell colspan="2" halign="left"> ~${versionNumber} ${versionType} <b>\t\t\t\t\t\t\t\t\t\t\t\t\t COMMUNITY SUPERVISION ~CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
						</font>
					</table-row>
					<table-row padding-top="5" padding-bottom="10">
						<font name="arial_10">
							<cell halign="left">
								~For: <if cond="${Name != null && Name != ""}"><u trim-body="true"> ~${Name}</u></if><else>_____________________</else>
							</cell>
							<cell halign="right">
								~Cause:  <if cond="${Cause != null && Cause != ""}"><u> ~${Cause}</u></if><else>_____________________</else>
							</cell>
						</font>
					</table-row>
				</table>				
		</header-part>
	</header>
	
	<table columns="2" border-style="none" widths="67,43" keep-together="true" line-spacing="1.25">
		<font name="arial">
			<table-row>
				<cell>THE STATE OF TEXAS</cell>
				<cell>IN THE COUNTY CRIMINAL COURT</cell>
			</table-row>
			<table-row>
				<cell></cell>
				<cell>NUMBER <if cond="${court != null && court != ""}"><u>${court}</u></if><else>__________</else></cell>
			</table-row>
			<table-row>
				<cell>VS.</cell>
				<cell>OF HARRIS COUNTY, TEXAS</cell>
			</table-row>
			<table-row>
				<cell><if cond="${Name != null && Name != ""}"><u trim-body="true">${Name}</u></if><else>_____________________</else></cell>
				<cell>CAUSE NUMBER <u>${cause}</u></cell>
			</table-row>	
		</font>	
	</table>
		
	<table columns="1" border-style="none" keep-together="true" spacing-before="10">
		<table-row halign="center">
			<font name="arial_10">		
				<cell halign="center">${versionNumber} ${versionType}</cell>
			</font>
		</table-row>
		<table-row halign="center">
			<font name="arial">
				<cell halign="center"><b>CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
			</font>
		</table-row>
	</table>
	
	<paragraph halign="justified" line-spacing="1.25" spacing-before="10" spacing-after="10">
		<font name="arial_10">On this the <if cond="${dopDayOrdinal != null}"><u>${dopDayOrdinal}</u> day of <u>${dopMonth}, ${dopYear}</u></if><else>__________________</else>, the Court finds that the Order placing the Defendant on community supervision in Cause No. <if cond="${Cause != null && Cause != ""}"><u>${Cause}</u></if><else>__________________</else>, should be, and the same is hereby amended by changing condition(s) of community supervision in said Order as follows, to wit;	
		</font>
	</paragraph>
	
	_if_ (${conditionsAdded} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10">
				<u><b>ADD CONDITION: </b></u>
			</font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="+">
			_foreach_(condition ${conditionsAdded} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>	
				</list-item>		
			_endfor_	
		</list>
	_endif_
	
	_if_ (${conditionsRemoved} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10">
				<u><b>DELETE CONDITION:</b></u>
			</font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="X">	
			_foreach_(condition ${conditionsRemoved} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>
				</list-item>
			_endfor_
		</list>
	_endif_
	
	_if_ (${conditionsUpdated} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10">
				<u><b>CHANGE CONDITION:</b></u>
			</font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="X">	
			_foreach_(condition ${conditionsUpdated} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>
				</list-item>
			_endfor_
		</list>
	_endif_
	
	<table columns="1" keep-together="true">
		<font name="arial_10">
			<table-row>
				<cell>All other terms and conditions of community supervision shall remain in full force and effect as heretofore ordered.
				</cell>
			</table-row>
			<table-row padding-top="15">
				<cell>Signed this <if cond="${signedDay != null && signedDay != ""}"><u>${signedDay}</u></if><else>___________</else> day of <if cond="${signedMonth != null && signedMonth != ""}"><u>${signedMonth}</u></if><else>___________</else> A.D. <if cond="${signedYear != null && signedYear != ""}"><u>${signedYear}</u></if><else>__________</else>
				</cell>
			</table-row>
			<table-row>
				<cell>
					<table columns="1" width="75" halign="right" spacing-before="30">
						<table-row padding-top="30">
							<cell halign="left">
								______________________________________________________________________
							</cell>
						</table-row>
						<table-row>
							<cell halign="left">
								<if cond="${signedByFLName != null && signedByFLName != ""}" trim-body="true">${signedByFLName},</if>
								<else>______________________________________</else> PRESIDING JUDGE
							</cell>
						</table-row>
					</table>
				</cell>
			</table-row>
		</font>
	</table>
	
	<table width="100" columns="3" halign="right" widths="25,14,61" keep-together="true">
		<font name="arial_10">
			<table-row padding-top="10">
				<cell colspan="3">
					<paragraph spacing-before="20" halign="justified" spacing-after="5">
						I acknowledge receipt of a copy of the conditions of my supervision as modified and further acknowledge that a Harris County Community Supervision Officer has explained said modification.
					</paragraph>
				</cell>
			</table-row>
			<table-row padding-top="5">
				<cell></cell>
				<cell></cell>
				<cell halign="left">
					Dated:<space/><if cond="${signedByDefendantDate != null && signedByDefendantDate != ""}"><u>${signedByDefendantDate}</u></if><else>__________________</else>
				</cell>
			</table-row>
			<table-row padding-top="30">
				<cell border-style="left|right|top" border-width="1"></cell>
				<cell></cell>
				<cell halign="left">________________________________________________________</cell>
			</table-row>
			<table-row>
				<cell border-style="left|right" border-width="1"></cell>
				<cell></cell>
				<cell halign="left">
					<if cond="${cloName != null && cloName != ""}" trim-body="true">${cloName},</if>
					<else>______________________________________</else> CLO / CSO OFFICER
				</cell>
			</table-row>
			<table-row padding-top="30">
				<cell border-style="left|right" border-width="1"></cell>		
				<cell></cell>
				<cell halign="left">________________________________________________________</cell>
			</table-row>
			<table-row>
				<cell border-style="left|right" border-width="1"></cell>		
				<cell></cell>
				<cell halign="left">
					<if cond="${name != null && name != ""}">${name},</if>
					<else>______________________________________</else> DEFENDENT
				</cell>
			</table-row>
			<table-row padding-top="5">
				<cell border-style="left|right" border-width="1"></cell>
				<cell></cell>
				<cell></cell>
			</table-row>		
			<table-row padding-top="5">
				<cell border-style="left|right|bottom" border-width="1"></cell>
				<cell></cell>
				<cell></cell>	
			</table-row>
			<table-row padding-top="0">
				<cell colspan="3" halign="left">
					Defendant's Right Thumbprint
				</cell>
			</table-row>
		</font>
	</table>

</document>