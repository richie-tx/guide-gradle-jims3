<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="50">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false" allow-printing="false"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="arial" family="arial" style="normal" size="12"/>
	<font-def name="arial_10" family="arial" style="normal" size="10"/>
	
	<header>
		<header-part halign="left"  width="510">
				<table width="510" columns="2">
					<table-row halign="center">
						<font name="arial">
							<cell colspan="2" halign="left"> ~${versionNumber} ${versionType} <b> ~CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
						</font>
					</table-row>
					<table-row padding-top="5" padding-bottom="10">
						<font name="arial_10">
							<cell halign="left">
								~For: <if cond="${Name != null && Name != ""}"><u trim-body="true"> ~${Name}</u></if><else>_____________________</else>
							</cell>
							<cell halign="right">
								~Cause:  <if cond="${Cause != null && Cause != ""}"><u> ~${Cause}</u></if><else>_____________________</else>
							</cell>
						</font>
					</table-row>
				</table>				
		</header-part>
	</header>
	
	<footer padding-top="10">
		<font name="arial_10">
			<footer-part halign="left" width="100">
				<table columns="1" width="80" border-style="none">
					<cell no-wrap="true" padding="10">${comments}</cell>
				</table>		
			</footer-part>
		</font>
	</footer>
	
	<table columns="2" border-style="none" widths="65, 35" keep-together="true" line-spacing="1.25">
		<font name="arial">
			<table-row halign="center" padding-bottom="5">
				<font name="arial_10">
					<cell colspan="2" halign="center">CAUSE NUMBER <if cond="${Cause != null && Cause != ""}"><u>${Cause}</u></if><else>_____________________</else></cell>
				</font>
			</table-row>
			<table-row>
				<cell>THE STATE OF TEXAS</cell>
				<cell>IN COUNTY CRIMINAL COURT</cell>
			</table-row>
			<table-row>
				<cell>VS.</cell>
				<cell>AT LAW NO. <if cond="${court != null && court != ""}"><u>${court}</u></if><else>__________________</else></cell>
			</table-row>
			<table-row>
				<cell><if cond="${Name != null && Name != ""}"><u trim-body="true">${Name}</u></if><else>__________________</else></cell>
				<cell>HARRIS COUNTY, TEXAS</cell>
			</table-row>	
		</font>
	</table>

	<table columns="2" border-style="none" widths="65, 35" spacing-before="10">
		<table-row halign="center">
			<font name="arial_10">
				<cell colspan="2" halign="center">${versionNumber} ${versionType}</cell>
			</font>
		</table-row>
		<table-row halign="center">
			<font name="arial">
				<cell colspan="2" halign="center"><b>CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
			</font>
		</table-row>
	</table>
		
	<paragraph halign="justified" line-spacing="1.25" spacing-before="15" spacing-after="10">
		<font name="arial_10">On this the <if cond="${dopDayOrdinal != null && dopDayOrdinal != ""}"><u>${dopDayOrdinal}</u> day of <u>${dopMonth}, ${dopYear}</u></if><else>__________________</else>, the defendant being granted <if cond="${termHCJ != null && termHCJ != ""}"><u>${termHCJ}</u></if><else>__________________</else> in the <b>Harris County Jail</b> probated <if cond="${supervisionLengthNum != null && supervisionLengthNum != ""}"><u>${supervisionLengthNum}</u></if><else>___________</else> Community Supervision for the misdemeanor offense of <if cond="${offense != null && offense != ""}"><u>${offense}</u></if><else>__________________</else> in accordance with section <u>7</u> of Article 42.12, of the Texas Code of Criminal Procedure is hereby ordered to abide by all of the following conditions and terms of Community Supervision during the period of community supervision.	
		</font>
	</paragraph>

	<table width="100" widths="5,7,92" padding-bottom="6">
		<font name="arial_10">
			_foreach_(eachCondition ${conditions} 'even' 'odd')
				<table-row>
					<cell halign="left" valign="top" colspan="1">${eachCondition.conditionNumber}</cell>
					<cell colspan="2">${eachCondition.conditionName}</cell>
				</table-row>
				<if cond="${eachCondition.subConditions != null && eachCondition.subConditions != ""}">
					_foreach_(subCondition ${eachCondition.subConditions} 'even' 'odd')
						<table-row>
							<cell halign="left" valign="top" colspan="1"></cell>
							<cell halign="left" valign="top" colspan="1">${subCondition.subConditionNumber}</cell>
							<cell halign="left" colspan="1">${subCondition.subConditionName}</cell>
						</table-row>
					_endfor_
				</if>
			_endfor_
		</font>
	</table>
	
	<new-page size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="30">
		<header>
			<header-part halign="left"  width="750" padding-bottom="10">
				<table width="100" columns="2" widths="55,45" >
					<table-row>
						<font name="arial">
							<cell><if cond="${versionNumber != null && versionNumber != ""}">${versionNumber} ${versionType}<space/></if><b>CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
						</font>
					</table-row>
					<table-row padding-top="5">
						<font name="arial_10">
							<cell halign="left">
								For: <if cond="${Name != null && Name != ""}"><u trim-body="true">${Name}</u></if><else>_____________________</else>
							</cell>
							<cell halign="left">
								Cause:  <if cond="${Cause != null && Cause != ""}"><u>${Cause}</u></if><else>_____________________</else>
							</cell>
						</font>
					</table-row>
				</table>				
			</header-part>
		</header>
		
		<footer padding-top="10">
			<font name="arial_10">
				<footer-part halign="left" width="100">
					<table columns="1" width="80" border-style="none">
						<cell no-wrap="true" padding="10">${comments}</cell>
					</table>		
				</footer-part>
			</font>
		</footer>
	</new-page>

	<table columns="1" width="100" halign="justified">
		<cell>
			<font name="arial_10">You are hereby advised that under the laws of this State, the Court shall determine the terms and conditions of Community Supervision, and may at any time during the period of Community Supervision, alter or modify the Conditions of Community Supervision.  
			</font>
		</cell>
		<cell padding-top="10">
			<font name="arial_10">The Court also has the authority at anytime during the period of Community Supervision to revoke the Community Supervision when a preponderance of the evidence establishes a violation of one or more of the conditions set forth above.  The Clerk of the Court has furnished me with a copy of the terms and conditions of Community Supervision.    
			</font>
		</cell>
	</table>

	<paragraph spacing-before="20">
		<font name="arial_10">Community Supervision expires the <if cond="${probationEndDay != null && probationEndDay != ""}"><u>${probationEndDay}</u></if><else>___________</else> day of <if cond="${probationEndMonth != null && probationEndMonth != ""}"><u>${probationEndMonth}</u></if><else>___________</else> A.D. <if cond="${probationEndYear != null && probationEndYear != ""}"><u>${probationEndYear}</u></if><else>___________</else>
		</font>
	</paragraph>
	
	<table columns="2" width="100" halign="left" spacing-before="30">
		<table-row>
			<cell padding-right="15">
				<font name="arial_10">
					___________________________________
				</font>
			</cell>
			<cell>
				<font name="arial_10">
					<if cond="${signedByDefendantDate != null && signedByDefendantDate != ""}"><u>${signedByDefendantDate}</u></if>
					<else>___________</else>
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell padding-right="15">
				<font name="arial_10">
					<if cond="${name != null && name != ""}">${name}</if>
					<else>_________________________________</else>, DEFENDANT
				</font>
			</cell>
			<cell>
				<font name="arial_10">DATE</font>
			</cell>
		</table-row>
	</table>
	
	<paragraph spacing-before="30">
		<font name="arial_10">
			\t\t\t\t\t<b>Signed this </b> <if cond="${signedDay != null && signedDay != ""}"><u>${signedDay}</u></if><else>___________________</else><b> day of </b><if cond="${signedMonth != null && signedMonth != ""}"><u>${signedMonth}</u></if><else>__________________________</else><b> A.D. </b><if cond="${signedYear != null && signedYear != ""}"><u>${signedYear}</u></if><else>_________________</else>
		</font>
	</paragraph>

	<table columns="1" width="75" halign="right" spacing-before="30">
		<table-row>
			<cell halign="left">
				<font name="arial_10">_____________________________________________________________________</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="left">
				<font name="arial_10">			
					<if cond="${signedByFLName != null && signedByFLName != ""}">${signedByFLName},</if>
					<else>________________________</else> PRESIDING JUDGE
				</font>
			</cell>
		</table-row>	
	</table>
	
	<table columns="2" width="75" halign="right" spacing-before="20" widths="65,35">
		<table-row>
			<cell halign="left">
				<font name="arial_10">___________________________________________</font>
			</cell>
			<cell padding-left="32" halign="left">
				<font name="arial_10">
					<if cond="${signedDate != null && signedDate != ""}"><u>${signedDate}</u></if>
					<else>__________________</else>
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="left">
				<font name="arial_10">
					<if cond="${cloName != null && cloName != ""}">${cloName},</if>
					<else>________________________</else> CLO/CSO OFFICER
				</font>
			</cell>
			<cell padding-left="32" halign="left">
				<font name="arial_10">DATE</font>
			</cell>
		</table-row>
	</table>

	<table columns="2" width="27" widths="7, 20" keep-together="true" spacing-before="30" halign="right" line-spacing="1.25">
		<font name="arial_10">
			<table-row>
				<cell halign="left">
					SPN: 
				</cell>
				<cell halign="left">
					<if cond="${spn != null && spn != ""}"><u>${spn}</u></if><else>__________________\t</else>
				</cell>
			</table-row>
			<table-row>
				<cell halign="left" padding-top="10">
					PLEA:
				</cell>
				<cell halign="left" padding-top="10">
					<if cond="${plea != null && plea != ""}"><u>${plea}</u></if><else>__________________\t</else>
				</cell>
			</table-row>
			<table-row>
				<cell halign="left" padding-top="10">
					FINE:
				</cell>
				<cell halign="left" padding-top="10">
					<if cond="${fineAmountTotal != null && fineAmountTotal != ""}"><u>$ ${fineAmountTotal}</u></if><else>__________________\t</else>
				</cell>
			</table-row>
		</font>
	</table>
		 
	<graphics stroke-color="black" line-width="1.0">
		<set-line-dash phase="1" units-on="0" units-off="0"/>
		<draw-rectangle x="50" y="130" width="130" height="130"/>
	</graphics>

	<table columns="1" width="75" halign="left" spacing-before="210">
		<font name="arial_10">
			<cell>Defendant's Right Thumbprint</cell>
		</font>
	</table>

	_if_ (${summaryofChanges} notEmpty)
	<if cond="${summaryofChanges != null && summaryofChanges != ""}">
		<table columns="1" border-style="box" keep-together="true" spacing-before="5" padding="10" >
			<font name="arial_10">
				<cell halign="justified">${summaryofChanges}</cell>
			</font>
		</table>
	</if>
	_endif_

</document>