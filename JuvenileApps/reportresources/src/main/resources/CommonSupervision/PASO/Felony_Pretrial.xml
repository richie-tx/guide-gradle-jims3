<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="50">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false" allow-printing="false"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="arial" family="arial" style="normal" size="12"/>
	<font-def name="arial_10" family="arial" style="normal" size="10"/>
	
	<header>
		<header-part halign="left"  width="510">
				<table width="510" columns="2">
					<table-row halign="center">
						<font name="arial">
							<cell colspan="2" halign="left"> ~${versionNumber} ${versionType} <b> ~CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
						</font>
					</table-row>
					<table-row padding-top="5" padding-bottom="15">
						<font name="arial_10">
							<cell halign="left">
								~For: <if cond="${Name != null}"><u trim-body="true"> ~${Name}</u></if><else>_____________________</else>
							</cell>
							<cell halign="right">
								~Cause:  <if cond="${Cause != null}"><u> ~${Cause}</u></if><else>_____________________</else>
							</cell>
						</font>
					</table-row>
				</table>				
		</header-part>
	</header>
	
	<footer padding-top="10">
		<font name="arial_10">
			<footer-part halign="left" width="100">
				<table columns="1" width="80" border-style="none">
					<cell no-wrap="true" padding="10">${comments}</cell>
				</table>		
			</footer-part>
		</font>
	</footer>

	<table columns="2" border-style="none" widths="61,49" keep-together="true" line-spacing="1.25">
		<font name="arial">
			<table-row halign="center" padding-bottom="5">
				<font name="arial_10">
					<cell colspan="2" halign="center"><b>Cause No. </b><if cond="${Cause != null}"><u>${Cause}</u></if><else>_____________________</else></cell>
				</font>
			</table-row>
			<table-row>
				<cell>THE STATE OF TEXAS</cell>
				<cell>IN THE <if cond="${court != null}"><u>${court}</u></if><else>__________________</else> DISTRICT COURT</cell>
			</table-row>
			<table-row>
				<cell>VS.</cell>
				<cell>OF HARRIS COUNTY, TEXAS</cell>
			</table-row>
			<table-row>
				<cell><u trim-body="true">${Name}</u></cell>
				<cell></cell>
			</table-row>
			<table-row halign="center">
				<font name="arial">
					<cell colspan="2" halign="center">${versionNumber} ${versionType}</cell>
				</font>
			</table-row>
			<table-row halign="center">
				<font name="arial">
					<cell colspan="2" halign="center"><b>ORDER</b></cell>
				</font>
			</table-row>	
		</font>	
	</table>

	<paragraph halign="justified" line-spacing="1.25" spacing-before="10" spacing-after="10">
		<font name="arial_10">\t\t\t\tUpon motion by the defendant, and agreement by the State of Texas, by and through the District Attorney of Harris County, Texas, the Court ORDERS that:	
		</font>	
	</paragraph>
	
	<table width="100" widths="5,7,92" padding-bottom="6">
		<font name="arial_10">
		_foreach_(eachCondition ${conditions} 'even' 'odd')
			<table-row>
				<cell halign="left" valign="top" colspan="1">${eachCondition.conditionNumber}</cell>
				<cell colspan="2">${eachCondition.conditionName}</cell>
			</table-row>
			<if cond="${eachCondition.subConditions != null}">
				_foreach_(subCondition ${eachCondition.subConditions} 'even' 'odd')
					<table-row>
						<cell halign="left" valign="top" colspan="1"></cell>
						<cell halign="left" valign="top" colspan="1">${subCondition.subConditionNumber}</cell>
						<cell halign="left" colspan="1">${subCondition.subConditionName}</cell>
					</table-row>
				_endfor_
			</if>
		_endfor_
		</font>
	</table>
	
	<paragraph spacing-before="20">
		<font name="arial_10">
			\t\t\t\t<b>Signed this </b> <if cond="${signedDay != null && signedDay != ""}"><u>${signedDay}</u></if><else>___________</else><b> day of </b><if cond="${signedMonth != null && signedMonth != ""}"><u>${signedMonth}</u></if><else>___________</else><b> A.D. </b><if cond="${signedYear != null && signedYear != ""}"><u>${signedYear}</u></if><else>__________</else>
		</font>
	</paragraph>

	<table spacing-before="40" columns="2" widths="85, 15" halign="left">
		<table-row>
			<cell halign="left">
				<font name="arial_10">________________________________________</font>
			</cell>
			<cell><u>Right Thumbprint</u></cell>
		</table-row>
		<table-row>
			<cell halign="left">
				<font name="arial_10">JUDGE PRESIDING</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="left">
				<font name="arial_10">in the <if cond="${court != null}"><u>${court}</u></if><else>__________________</else> District Court
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell halign="left">
				<font name="arial_10">Harris County, Texas
				</font>
			</cell>
		</table-row>
	</table>

</document>