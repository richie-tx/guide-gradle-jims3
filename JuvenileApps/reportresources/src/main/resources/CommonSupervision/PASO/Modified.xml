<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="50">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false" allow-printing="false"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="arial" family="arial" style="normal" size="12"/>
	<font-def name="arial_10" family="arial" style="normal" size="10"/>
	
	<header>
		<header-part halign="left"  width="510">
				<table width="510" columns="2">
					<table-row halign="center">
						<font name="arial">
							<cell colspan="2" halign="left"> ~${versionNumber} ${versionType} <b>\t\t\t\t\t\t\t\t\t\t\t\t\t COMMUNITY SUPERVISION ~CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
						</font>
					</table-row>
					<table-row padding-top="5" padding-bottom="10">
						<font name="arial_10">
							<cell halign="left">
								~For: <if cond="${Name != null && Name != ""}"><u trim-body="true"> ~${Name}</u></if><else>_____________________</else>
							</cell>
							<cell halign="right">
								~Cause:  <if cond="${Cause != null && Cause != ""}"><u> ~${Cause}</u></if><else>_____________________</else>
							</cell>
						</font>
					</table-row>
				</table>				
		</header-part>
	</header>
	
	<footer padding-top="10">
		<font name="arial_10">
			<footer-part halign="left" width="100">
				<table columns="1" width="80" border-style="none">
					<cell no-wrap="true" padding="10">${comments}</cell>
				</table>		
			</footer-part>
		</font>
	</footer>

	<table columns="2" border-style="none" widths="55,50" keep-together="true" line-spacing="1.25">
		<font name="arial">
			<table-row>
				<cell>THE STATE OF TEXAS</cell>
				<cell>IN THE <if cond="${court != null && court != ""}"><u>${court}</u></if><else>__________________</else> DISTRICT COURT OF</cell>
			</table-row>
			<table-row>
				<cell>VS.</cell>
				<cell>HARRIS COUNTY, TEXAS</cell>
			</table-row>
			<table-row>
				<cell><u trim-body="true">${Name}</u></cell>
				<cell>CAUSE NUMBER <if cond="${Cause != null && Cause != ""}"><u>${Cause}</u></if><else>_____________________</else></cell>
			</table-row>	
		</font>	
	</table>

	<table columns="2" border-style="none" widths="65, 35" spacing-before="10">
		<table-row halign="center">
			<font name="arial_10">
				<cell colspan="2" halign="center">${versionNumber} ${versionType}</cell>
			</font>
		</table-row>
		<table-row halign="center">
			<font name="arial">
				<cell colspan="2" halign="center"><b>CONDITIONS OF COMMUNITY SUPERVISION</b></cell>
			</font>
		</table-row>
	</table>
	
<paragraph halign="justified" line-spacing="1.35" spacing-before="10" spacing-after="10">
		<font name="arial_10">On this the <if cond="${dopDayOrdinal != null && dopDayOrdinal != ""}"><u>${dopDayOrdinal}</u> day of <u>${dopMonth}, ${dopYear}</u></if><else>__________________</else>, the Court finds that the Order placing the Defendant on community supervision in Cause No. <if cond="${Cause != null && Cause != ""}"><u>${Cause}</u></if><else>__________________</else>, should be, and the same is hereby amended by changing condition(s) of community supervision in said Order as follows, to wit;	
		</font>
	</paragraph>

	_if_ (${conditionsAdded} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10">
				<u><b>ADD CONDITION: </b></u>
			</font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="+">
			_foreach_(condition ${conditionsAdded} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>	
				</list-item>		
			_endfor_	
		</list>
	_endif_
	
	_if_ (${conditionsRemoved} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10"><u><b>DELETE CONDITION:</b></u></font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="X">	
			_foreach_(condition ${conditionsRemoved} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>
				</list-item>
			_endfor_
		</list>
	_endif_
	
	_if_ (${conditionsUpdated} notEmpty)
		<paragraph halign="justified" spacing-after="10">
			<font name="arial_10">
				<u><b>CHANGE CONDITION:</b></u>
			</font>
		</paragraph>

		<list numbered="false" lettered="false" symbol-indent="15" line-spacing="1.25" type="X">	
			_foreach_(condition ${conditionsUpdated} 'even' 'odd')
				<list-item>
					<font name="arial_10">${condition}</font><br/><br/>
				</list-item>
			_endfor_
		</list>
	_endif_
	
	<table columns="2" widths="25, 75" keep-together="true">
		<table-row>
			<cell colspan="2" halign="justified" line-spacing="1.35">
				<font name="arial_10">All other terms and conditions of community supervision shall remain in full force and effect as heretofore ordered.	
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell colspan="2" halign="justified" line-spacing="1.25" padding-top="10" padding-bottom="30">
				<font name="arial_10"><b>\t\t\t\t\tSigned this </b> <if cond="${signedDay != null && signedDay != ""}"><u>${signedDay}</u></if><else>___________</else><b> day of </b><if cond="${signedMonth != null && signedMonth != ""}"><u>${signedMonth}</u></if><else>___________</else><b> A.D. </b><if cond="${signedYear != null && signedYear != ""}"><u>${signedYear}</u></if><else>__________</else>
				</font>
			</cell>
		</table-row>
		<table-row>
			<cell></cell>
			<cell halign="left">
				______________________________________________________________________
			</cell>
		</table-row>
		<table-row>
			<cell></cell>
			<cell halign="left">
				<font name="arial_10">
					<if cond="${signedByFLName != null && signedByFLName != ""}">${signedByFLName},</if>
					<else>______________________________________</else>
				</font> PRESIDING JUDGE
			</cell>
		</table-row>
	</table>
	
	<table width="100" columns="3" widths="25,14,61" keep-together="true">
		<table-row>
			<cell colspan="3" padding-top="20">
				<paragraph halign="justified" spacing-after="5" line-spacing="1.35">
					<font name="arial_10">I acknowledge receipt of a copy of the conditions of my supervision as modified and further acknowledge that a Harris County Community Supervision Officer has explained said modification.
					</font>
				</paragraph>
			</cell>
		</table-row>
		<table-row padding-top="5">
			<cell></cell>
			<cell></cell>
			<cell halign="left">
				<font name="arial_10">
				Dated: <if cond="${signedByDefendantDate != null && signedByDefendantDate != ""}"><u>${signedByDefendantDate}</u></if><else>__________________</else>
				</font>
			</cell>
		</table-row>
		<table-row padding-top="30">
			<cell border-style="top|left|right" border-width="1"></cell>
			<cell></cell>
			<cell halign="left">_________________________________________________________</cell>
		</table-row>
		<table-row>
			<cell border-style="left|right" border-width="1"></cell>
			<cell></cell>
			<cell halign="left">
				<font name="arial_10">
					<if cond="${cloName != null && cloName != ""}">${cloName},</if>
					<else>________________________________</else>
				</font> CLO / CSO OFFICER
			</cell>
		</table-row>
		<table-row padding-top="30">
			<cell border-style="left|right" border-width="1"></cell>		
			<cell></cell>
			<cell halign="left">_________________________________________________________</cell>
		</table-row>
		<table-row>
			<cell border-style="left|right" border-width="1"></cell>		
			<cell></cell>
			<cell halign="left">
				<font name="arial_10">
					<if cond="${name != null && name != ""}">${name},</if>
					<else>______________________________________</else>
				</font> DEFENDENT
			</cell>
		</table-row>
		<table-row padding-top="5">
			<cell border-style="left|right" border-width="1"></cell>
			<cell></cell>
			<cell></cell>
		</table-row>		
		<table-row padding-top="5">
			<cell border-style="left|right|bottom" border-width="1"></cell>
			<cell></cell>
			<cell></cell>	
		</table-row>
		<table-row padding-top="0">
			<cell colspan="3" halign="left" valign="top">
				<font name="arial_10">Defendant's Right Thumbprint</font>
			</cell>
		</table-row>
	</table>

</document>