<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="55" margin-bottom="50">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="true"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>

	<font-def name="times" family="Times" style="normal" size="12"/>
	<font-def name="times_10" family="Times" style="normal" size="10"/>

	<header>
		<header-part halign="left"  width="510">
			<table width="510" columns="2">
				<table-row halign="center">
					<font name="times">
						<cell colspan="2" halign="left"> ~${versionNumber} ${versionType}<space/><b>~ COURTESY SUPERVISION GUIDELINES - INTERSTATE TRANSFER</b></cell>
					</font>
				</table-row>
				<table-row padding-top="5" padding-bottom="10">
					<font name="times_10">
						<cell halign="left">
							~For: <if cond="${Name != null && Name != ""}"><u trim-body="true"> ~${Name}</u></if><else>_____________________</else>
						</cell>
						<cell halign="right">
							~Cause:  <if cond="${Cause != null && Cause != ""}"><u> ~${Cause}</u></if><else>_____________________</else>
						</cell>
					</font>
				</table-row>
			</table>				
		</header-part>
	</header>
	
	<footer padding-top="10">
		<font name="times_10">
			<footer-part halign="left" width="100">
				<table columns="1" width="80" border-style="none">
					<cell no-wrap="true" padding="10">${comments}</cell>
				</table>		
			</footer-part>
		</font>
	</footer>
	
	<paragraph halign="center">
			<image source="images/CSCDCounty.jpg" width="510" height="85" scale-to-fit="false"/>
	</paragraph>
		
	<table columns="1" border-style="none" keep-together="true">
		<table-row halign="center">
			<font name="times">		
				<cell halign="center">${versionNumber} ${versionType}</cell>
			</font>
		</table-row>
		<table-row halign="center">
			<font name="times">
				<cell halign="center"><b>COURTESY SUPERVISION GUIDELINES</b></cell>
			</font>
		</table-row>
		<table-row halign="center">
			<font name="times">
				<cell halign="center"><b>INTERSTATE TRANSFER</b></cell>
			</font>
		</table-row>
	</table>		
		
	<paragraph halign="justified" line-spacing="1.25" spacing-before="10" spacing-after="10">
	<font name="times_10"><b>I, <u trim-body="true">${Name}</u>, understand that I will be supervised by the Harris County Community Supervision and Corrections Department as a courtesy to my original supervisory authority and to myself.  I further understand that I am obligated to fully comply with the following requirements in order to retain the privilege of having my supervision supervised by the Harris County Community Supervision and Corrections Department.</b>
	</font>	
	</paragraph>

	<table width="100" widths="3, 5,7,85" padding-bottom="6">
		<font name="times_10">
		_foreach_(eachCondition ${conditions} 'even' 'odd')
			<table-row>
				<cell></cell>
				<cell halign="left" valign="top" colspan="1">${eachCondition.conditionNumber}</cell>
				<cell colspan="2">${eachCondition.conditionName}</cell>
			</table-row>
			<if cond="${eachCondition.subConditions != null}">
				_foreach_(subCondition ${eachCondition.subConditions} 'even' 'odd')
					<table-row>
						<cell></cell>
						<cell halign="left" valign="top" colspan="1"></cell>
						<cell halign="left" valign="top" colspan="1">${subCondition.subConditionNumber}</cell>
						<cell halign="left" colspan="1">${subCondition.subConditionName}</cell>
					</table-row>
				_endfor_
			</if>
		_endfor_
		</font>
	</table>
	
	<table columns="2" border-style="none" keep-together="true" spacing-before="20" line-spacing="1.5">
		<font name="times_10">
		<table-row>
			<cell colspan="2">
				<b>I understand that my failure to comply with the above requirements may result in the return of my supervision to the original supervisory authority.</b>
			</cell>
		</table-row>
		<table-row>
			<cell colspan="2" padding-top="20" padding-bottom="30">
				<b>Signed this </b> <if cond="${signedDay != null && signedDay != ""}"><u>${signedDay}</u></if><else>___________</else><b> day of </b><if cond="${signedMonth != null && signedMonth != ""}"><u>${signedMonth}</u></if><else>___________</else><b> A.D. </b><if cond="${signedYear != null && signedYear != ""}"><u>${signedYear}</u></if><else>___________</else>
			</cell>
		</table-row>
		<table-row halign="left" valign="middle">
			<cell halign="left">______________________________</cell>
			<cell halign="left"><if cond="${spn != null && spn != ""}"><u>${spn}</u></if><else>______________________________</else></cell>
		</table-row>
		<table-row halign="left" valign="middle">
			<cell halign="left">Offender's Signature </cell>
			<cell halign="left">SPN #</cell>
		</table-row>				
		<table-row halign="center">
			<cell halign="left" padding-top="30">______________________________</cell>
			<cell halign="left" padding-top="30">______________________________</cell>
		</table-row>
		<table-row>
			<cell halign="left">Supervising Officer/Witness</cell>
			<cell halign="left">POI</cell>
		</table-row>
		</font>
	</table>		
				
				
	_if_ (${summaryofChanges} notEmpty)
	<if cond="${summaryofChanges != null}">
		<table columns="1" border-style="box" keep-together="true" spacing-before="20" padding="10" >
			<font name="times_10">
				<cell halign="justified">${summaryofChanges}</cell>
			</font>
		</table>
	</if>
	_endif_					
</document>