<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="25" margin-right="25" margin-top="25"  margin-bottom="25" height="100">
<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="true"/>
<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="${orderTitle}"/>
<viewer-preferences page-mode="NoThumbs" zoom="1"/>
<module>
<custom-tag alias="strong" tag="b">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="em" tag="i">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="ul" tag="list">
<attribute name="body" value="${body}"/>
</custom-tag>

<custom-tag alias="ol" tag="ul">
<attribute name="body" value="${body}"/>
<attribute name="numbered" value="true"/>
</custom-tag>
 
<custom-tag alias="li" tag="list-item">
<attribute name="body" value="${body}"/>
</custom-tag>

</module>

	<font-def name="helvetica" family="Helvetica" style="normal" size="8"/>
	<font-def name="geneva" family="georgia" style="normal" size="8"/>
	<font-def name="arial" family="arial" style="normal" size="8"/>
	<font-def name="times" family="Times" style="normal" size="8"/>

	<header>
	</header>		
	<footer padding-top="10">
		<font name="times" size="10" color="black">
		<footer-part padding-top="10" halign="center">Page : <b>${pageNumber , nointrprt}</b> of <b>${pageCount , nointrprt}</b></footer-part>
		</font>
	</footer>
	<table columns="3" border-style="none" keep-together="true" spacing-before="52" line-spacing="1.5" padding="1" width="100">
			<table-row halign="center" valign="middle">
			<if cond="${reAssessment}">
				<cell halign="left" colspan="2"><font name="times" size="8" color="black">TAPC CASE CLASSIFICATION \nRisk/Needs/Plan</font>
				</cell>				
			</if>
			</table-row>
			<if cond="${reAssessment}">
			<table-row halign="center" valign="middle">
					<cell halign="left"><font name="times" size="8" color="black">Form Number 30-41</font></cell>
					<cell halign="center"><b>${pageHeading}</b></cell>				
					<cell halign="right"><font name="times" size="8" color="black">Revised 10/88</font></cell>
			</table-row>
			</if>
			<if cond="${reAssessment == 'false'}">
			<table-row halign="center" valign="middle">
					<cell halign="left"></cell>
					<cell halign="center"><b>${pageHeading}</b></cell>				
					<cell halign="left"></cell>
			</table-row>
			</if>			
		</table>
	<table columns="20" border-style="none" keep-together="false" spacing-before="10" line-spacing="1.3" padding="1" width="100">
		<font name="times" size="8" style="normal">
			<table-row halign="left" valign="middle">
				<cell halign="left" colspan="4">DATE:<u><b>${assessementDate}</b></u></cell>
				<cell halign="left" colspan="5">SPN:<u><b>${spn}</b></u></cell>
				<cell halign="left" colspan="8">DEFENDANT:<u><b>${defendantName}</b></u></cell>
				<cell halign="right" colspan="3"></cell>
			</table-row>
			<table-row halign="left" valign="middle">
				<cell halign="left" colspan="4"></cell>
				<cell halign="left" colspan="5"></cell>
				<cell halign="left" colspan="8"></cell>
				<cell halign="right" colspan="3"><b>RISK SCORES</b></cell>
			</table-row>
		</font>
		<table-row halign="left" valign="middle">
			<cell halign="left" colspan="20"></cell>
		</table-row>
	<font name="times" size="10" style="normal">
		_foreach_(question ${riskAssessmentQuestionList} 'even' 'odd')				
			_foreach_(question1 ${question.questions} 'even' 'odd')	
			<if cond="${question1.responseChoices != null}">
			<table-row halign="left" valign="top">	
				<cell halign="left" colspan="8">
					_foreach_(qText ${question1.FormattedQText} 'even' 'odd')
						 ${qText}\n
					_endfor_
				</cell>
				<cell halign="left" colspan="10">
				_foreach_(choices ${question1.responseChoices} 'even' 'odd')	
					<if cond="${choices.responseValue != ''}">
						<table columns="20" border-style="none" line-spacing="1.3" keep-together="true" width="100">
							<table-row halign="left" valign="top">
								<cell halign="left">
									${choices.responseValue}  
								</cell>
								<cell halign="left" colspan="19">
									${choices.responseDisplayText}\n
								</cell>
							</table-row>
						</table>
					</if> 
				_endfor_
				</cell>
				<cell halign="center" valign="bottom" colspan="2">
					<u>${question1.responseText}</u>
				</cell>
			</table-row>
			</if>
			<if cond="${question1.responseChoices == null}">
			<table-row halign="left" valign="middle" height="15">
				<cell halign="center" colspan="20">
					<b> RATE THE FOLLOWING BASED ON PERIOD SINCE LAST CLASSIFICATION </b>
				</cell>
			</table-row>
			</if>
			_endfor_
		_endfor_
	
			<table-row halign="left" valign="middle">
					<cell halign="left" colspan="4"></cell>
					<cell halign="left" colspan="9"></cell>
					<cell halign="right" colspan="5"><b>TOTAL RISK SCORE=</b></cell>
					<cell halign="center" colspan="2"><u>${totalRiskScore}</u></cell>
			</table-row>
			<table-row halign="left" valign="middle">
					<cell halign="left" colspan="3"></cell>
					<cell halign="left" colspan="10"><b>RISK</b></cell>
					<cell halign="right" colspan="5"><b>RISK LEVEL=</b></cell>
					<cell halign="center" colspan="2"><u>${RiskLevel}</u></cell>
			</table-row>
			<table-row halign="left" valign="middle">
					<cell halign="left" colspan="4"></cell>
					<cell halign="left" colspan="16">2.Maximum(15+) \t\t 3.Medium(8-14) \t\t 4.Minimum(0-7)</cell>					
			</table-row>
			
	</font>
	</table>
	
	<new-page/>
	
	<table columns="1" border-style="none" keep-together="true" spacing-before="52" line-spacing="1.5" padding="1" width="100">
			<table-row halign="center" valign="middle">
				<if cond="${reAssessment}">
					<cell halign="center"><b>NEEDS REASSESSMENT</b></cell>
				</if>
				<if cond="${reAssessment == 'false'}">
					<cell halign="center"><b>NEEDS ASSESSMENT</b></cell>
				</if>
			</table-row>
	</table>
	
	<table columns="38" border-style="none" keep-together="false" spacing-before="10" line-spacing="1.3" padding="1" width="100">
		<font name="times" size="8" style="normal">
			<table-row halign="left" valign="middle">
				<cell halign="left" colspan="33">DEFENDANT:<b>${defendantName}</b></cell>
				<cell halign="right" colspan="5"><b>NEEDS SCORES</b></cell>
			</table-row>
		</font>
		<table-row halign="left" valign="middle">
			<cell halign="left" colspan="36"></cell>
		</table-row>
		<table-row halign="left" valign="middle">
			<cell halign="left" colspan="36"></cell>
		</table-row>
		<table-row halign="left" valign="middle">
			<cell halign="left" colspan="36"></cell>
		</table-row>		
	<font name="times" size="8" style="normal">
		
		_foreach_(question ${needAssessmentQuestionList} 'even' 'odd')			
			
			_foreach_(question1 ${question.questions} 'even' 'odd')	
			<table-row halign="left" valign="top">	
				<cell halign="left" colspan="37">					
					<b> ${question1.questionText}</b>					
				</cell>
			</table-row>			
			<table-row halign="right" valign="top">					
				<cell halign="left" colspan="1"/>
				_foreach_(choices ${question1.responseChoices} 'even' 'odd')	
					<if cond="${choices.responseValue != ''}">
								<cell halign="left" colspan="1">									
								</cell>					
								<cell halign="left" colspan="1">
									${choices.responseValue}
								</cell>
								<cell halign="left" colspan="6">
									${choices.responseDisplayText}
								</cell>						
					</if>				
				_endfor_				
				<cell halign="center" valign="bottom" colspan="5">
					<u>${question1.responseText}</u>
				</cell>
			</table-row>
			<table-row halign="left" valign="top">
				<cell halign="left" colspan="36">
				</cell>
			</table-row>					
			_endfor_			
		_endfor_
					
			<table-row halign="left" valign="middle">
				<cell halign="center" colspan="26">OVERRIDE</cell>
				<cell halign="left" colspan="7"><b>TOTAL NEEDS SCORE=</b></cell>
				<cell halign="center" colspan="5"><u>${totalNeedsScore}</u></cell>
			</table-row>
			<table-row halign="left" valign="middle">
				<cell halign="left" colspan="26"><b>ASSIGNED LEVEL OF SUPERVISION\t2.Maximum___\t3.Medium____\t4.Minimum_____</b></cell>
				<cell halign="left" colspan="7"><b>NEEDS LEVEL=</b></cell>
				<cell halign="center" colspan="5"><u>${needsLevel}</u></cell>			
			</table-row>
			<table-row halign="left" valign="middle">
				<cell halign="right" colspan="29"></cell>
				<cell halign="left" colspan="8"><b>\tNEEDS</b></cell>
			</table-row>
			<table-row halign="left" valign="middle">
				<cell halign="right" colspan="29"></cell>
				<cell halign="left" colspan="8"><b>2- Maximum(30+)</b></cell>
			</table-row>
			<table-row halign="left" valign="middle">
				<cell halign="right" colspan="29"></cell>
				<cell halign="left" colspan="8"><b>3- Meduim(15-29)</b></cell>
			</table-row>
			<table-row halign="left" height="45" valign="top">
				<cell halign="right" colspan="29"></cell>
				<cell halign="left" colspan="8"><b>4- Minimum(14 and Below)</b></cell>
			</table-row>
			<table-row halign="left" valign="top">
				<cell halign="left" colspan="23">
				</cell>
				<cell halign="left" colspan="15" border-style="top" valign="top">
					<b>Community Supervision Officer’s Signature</b>
				</cell>				
			</table-row>			
	</font>
	</table>	
</document>