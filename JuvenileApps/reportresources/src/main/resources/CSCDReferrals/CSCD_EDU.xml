<?xml version="1.0" encoding="US-ASCII"?>
<document size="LETTER" margin-left="50" margin-right="50" margin-top="0" margin-bottom="50" height="100">

	<encryption allow-fillin="false" allow-modify-contents="false" allow-degraded-printing="true" allow-screen-readers="false" allow-copy="false"	allow-printing="false"/>
	<meta author="JIMS2, Harris County" creator="JIMS2, Harris County" title="Referral Forms"/>
	<viewer-preferences page-mode="NoThumbs" zoom="1"/>
	
	<module>
		<font-def name="title" family="arial" style="bold" size="14"/>
		<font-def name="header" family="arial" style="normal" size="10"/>
		<font-def name="label" family="arial" style="bold" size="8"/>
		<font-def name="text" family="arial" style="normal|underline" size="8"/>
		<font-def name="times_8" family="Times" style="normal" size="8"/>
		
		<custom-tag alias="shr-title" tag="phrase">
			<attribute name="font" value="title"/>
		</custom-tag>
		
		<custom-tag alias="shr-header" tag="phrase">
			<attribute name="font" value="header"/>
		</custom-tag>
		
		<custom-tag alias="shr-label" tag="phrase">
			<attribute name="font" value="label"/>
		</custom-tag>
		
		<custom-tag alias="shr-text" tag="phrase">
			<attribute name="font" value="text"/>
		</custom-tag>
		
		<custom-tag alias="ul" tag="list">
			<attribute name="body" value="${body}"/>
		</custom-tag>
		
		<custom-tag alias="ol" tag="ul">
			<attribute name="body" value="${body}"/>
			<attribute name="numbered" value="true"/>
		</custom-tag>
		 
		<custom-tag alias="li" tag="list-item">
			<attribute name="body" value="${body}"/>
		</custom-tag>
	</module>
	
	<header>
		<font name="title">
		<paragraph halign="center">
			<image source="images/CSCDCounty.jpg" width="510" height="85" scale-to-fit="false"/>
		</paragraph>
		</font>
	</header>	
	
	<footer padding-top="10">
		<footer-part padding-top="15" halign="left" width="500">
			<table columns="3" border-style="none" widths="45, 10, 45" keep-together="false" line-spacing="1.25">
				<table-row>
					<cell colspan="3">
						<table columns="9" widths="12, 10, 11, 4, 13, 4, 14, 4, 28">
						<font name="times_8">
							<cell><b>Date of API:</b></cell>
							<cell><if cond="${apiDate!= null && apiDate!= ""}"><shr-text>${apiDate}</shr-text></if>
								<else><font name="times_8">____________</font></else>
							</cell>
							<cell>Vocabulary:</cell>
							<cell halign="right"><if cond="${apiVocabularyScore!= null && apiVocabularyScore!= ""}"><shr-text>${apiVocabularyScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell>\tComprehension:</cell>
							<cell halign="right"><if cond="${apiComprehensiveScore!= null && apiComprehensiveScore!= ""}"><shr-text>${apiComprehensiveScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell>\tAPI Average:</cell>
							<cell halign="right"><if cond="${apiAverageScore!= null && apiAverageScore!= ""}"><shr-text>${apiAverageScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell></cell>
						</font>
						</table>
					</cell>
				</table-row>
				<table-row>
					<cell colspan="3">
						<table columns="9" widths="12, 10, 11, 4, 13, 4, 14, 4, 28" padding-top="5">
						<font name="times_8">
							<cell><b>Date of TABE:</b></cell>
							<cell><if cond="${tabeDate!= null && tabeDate!= ""}"><shr-text>${tabeDate}</shr-text></if>
								<else><font name="times_8">____________</font></else>
							</cell>
							<cell>Comprehension:</cell>
							<cell halign="right"><if cond="${tabeComprehensiveScore!= null && tabeComprehensiveScore!= ""}"><shr-text>${tabeComprehensiveScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell>\tMathematics:</cell>
							<cell halign="right"><if cond="${tabeMathematicsScore!= null && tabeMathematicsScore!= ""}"><shr-text>${tabeMathematicsScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell>\tTABE Average:</cell>
							<cell halign="right"><if cond="${tabeAverageScore!= null && tabeAverageScore!= ""}"><shr-text>${tabeAverageScore}</shr-text></if>
								<else><font name="times_8">_____</font></else>
							</cell>
							<cell></cell>
						</font>
						</table>
					</cell>
				</table-row>
				<table-row halign="left" valign="middle">
					<cell padding-top="30" halign="left">________________________________________</cell>
					<cell padding-top="30" halign="left">_________</cell>
					<cell padding-top="30" halign="left">________________________________________</cell>
				</table-row>
				<table-row halign="left" valign="middle">
					
					<cell halign="left"><shr-header>Defendant Signature</shr-header></cell>
					<cell halign="left"><shr-header>Date</shr-header></cell>
					<cell halign="left"><shr-header>Evaluator</shr-header></cell>
					
				</table-row>
				<table-row>
					<cell padding-top="15" colspan="3">
						<font name="times_8">REPORT WITH THIS FORM AND A COPY OF CONDITIONS OF COMMUNITY SUPERVISION TO:</font>
					</cell>
				</table-row>
				<table-row>
					<cell colspan="3">
						<font name="times_8"><b>49 SAN JACINTO, 4TH FLOOR, ROOM 447</b></font>
					</cell>
				</table-row>
				<table-row>
					<cell colspan="3">
						<font name="times_8"><b>Monday through Friday 7:00 a.m. to 4:00 p.m.</b></font>
					</cell>
				</table-row>			
			</table>	
		</footer-part>
	</footer>
	
	<table columns="1" border-style="none" keep-together="true" spacing-after="5">
		<table-row halign="center">
			<font name="title">
				<cell halign="center" padding-top="10" padding-bottom="5">CSCD - Education Referral Form</cell>
			</font>
		</table-row>
		<table-row>
		<cell>
			<shr-label>CLIENT INFORMATION</shr-label>
		</cell>
		</table-row>
	</table>
	
	<table width="100" columns="3" widths="60,20,20"  padding="2" spacing-after="5">
		<table-row halign="left">
			<cell><shr-label>Name of Client:</shr-label></cell>
			<cell><shr-label>Client Phone:</shr-label></cell>
			<cell><shr-label>Date of Birth:</shr-label></cell>
		</table-row>
		<table-row padding-top="5" halign="left">
			<cell>
				<if cond="${superviseeName!= null && superviseeName!= ""}"><shr-text>${superviseeName}</shr-text></if>
				<else><font name="times_8">____________________________________________</font></else>
			</cell>
			<cell>
				<if cond="${superviseePhone!= null && superviseePhone!= ""}"><shr-text>${superviseePhone}</shr-text></if>
				<else><font name="times_8">______________</font></else>
			</cell>
			<cell>
				<if cond="${superviseeDOB!= null && superviseeDOB!= ""}"><shr-text>${superviseeDOB}</shr-text></if>
				<else><font name="times_8">______________</font></else>
			</cell>
		</table-row>
	</table>	
		
	<table width="100" columns="4" widths="3,15,15,67" halign="left" padding="2" spacing-after="10">
		<cell></cell>
		<cell>
			<shr-label trim-body="true">Court:<space/></shr-label>
			<if cond="${court!= null && court!= ""}"><shr-text>${court}</shr-text></if>
			<else><font name="times_8">____</font></else>
		</cell>
		<cell>
			<shr-label trim-body="true">SPN:<space/></shr-label>
			<if cond="${superviseeId!= null && superviseeId!= ""}"><shr-text>${superviseeId}</shr-text></if>
			<else><font name="times_8">____</font></else>
		</cell>
		<cell>
			<shr-label trim-body="true">Current Offense:<space/></shr-label>
			<if cond="${currentOffense!= null && currentOffense!= ""}"><shr-text>${currentOffense}</shr-text></if>
			<else><font name="times_8">____________________________________________________________</font></else>
		</cell>
	</table>	
	
	<table width="100" columns="4" widths="39,20,11,30" spacing-after="10">
		<table-row>
			<cell border-style="top" border-width="1"><shr-label>Referring Source:</shr-label></cell>
			<cell border-style="top" border-width="1"><shr-label>POI</shr-label></cell>
			<cell border-style="top" border-width="1"><shr-label>Date</shr-label></cell>
			<cell border-style="top" border-width="1"><shr-label>Region</shr-label></cell>
		</table-row>
		<table-row halign="left">
			<cell>
				<shr-header trim-body="true">Officer Name:<space/></shr-header>
				<if cond="${supervisionStaffName!= null && supervisionStaffName!= ""}"><font name="text">${supervisionStaffName}</font></if>
				<else><font name="times_8">______________________________</font></else>
			</cell>
			<cell>
				<if cond="${staffPositionName!= null && staffPositionName!= ""}"><font name="text">${staffPositionName}</font></if>
				<else><font name="times_8">____</font></else>
			</cell>
			<cell>
				<if cond="${referralDateAsStr!= null && referralDateAsStr!= ""}"><font name="text">${referralDateAsStr}</font></if>
				<else><font name="times_8">__________</font></else>
			</cell>
			<cell>
				<if cond="${superviseeProgramUnit!= ""}"><font name="text">${superviseeProgramUnit}</font></if>
				<else><font name="times_8">________________________</font></else>
			</cell>
		</table-row>
		<table-row halign="left" >
			<cell>
				<shr-label trim-body="true">Contact Information:\tPhone:<space/></shr-label>
				<if cond="${supervisionStaffPhone!= null && supervisionStaffPhone!= ""}"><font name="text">${supervisionStaffPhone}</font></if>
				<else><font name="times_8">____________________</font></else>
			</cell>
			<cell>
				<shr-label trim-body="true">Fax:<space/></shr-label>
				<if cond="${supervisionStaffFax!= null && supervisionStaffFax!= ""}"><font name="text">${supervisionStaffFax}</font></if>
				<else><font name="times_8">____________________</font></else>
			</cell>
			<cell colspan="2">
				<shr-label trim-body="true">Email Address:<space/></shr-label>
				<if cond="${superviseeStaffEmail!= null && superviseeStaffEmail!= ""}"><font name="text">${superviseeStaffEmail}</font></if>
				<else><font name="times_8">_________________________________________</font></else>
			</cell>
		</table-row>
	</table>
	
	<table width="100" columns="2" widths="55,45" padding="0">
		<table-row halign="left">
			<cell border-style="top" border-width="1" valign="bottom">
				<shr-label trim-body="true">Program Name:<space/></shr-label>
				<if cond="${programName!= null && programName!= ""}"><shr-text>${programName}</shr-text></if>
				<else><font name="times_8">______________________________________________________</font></else>
			</cell>
			<cell border-style="top" border-width="1" valign="bottom">
				<shr-label trim-body="true">Court Order or Volunteer:<space/></shr-label>
				<if cond="${adultEducationOrderVolunteer!= null && adultEducationOrderVolunteer!= ""}"><shr-text>${adultEducationOrderVolunteer}</shr-text></if>
				<else><font name="times_8">_________________________________</font></else>
			</cell>
		</table-row>
		<table-row halign="left">
			<cell valign="top">
				<shr-label trim-body="true">Program Location:<space/></shr-label>
				<if cond="${referralProgramLocation!= null && referralProgramLocation!= ""}"><shr-text>${referralProgramLocation}</shr-text></if>
				<else><font name="times_8">________________________________________</font></else>
			</cell>
			<cell valign="top">
				<shr-label trim-body="true">Program Phone:<space/></shr-label>
				<if cond="${referralProgramPhone!= null && referralProgramPhone!= ''}"><shr-text>${referralProgramPhone}</shr-text></if>
				<else><font name="times_8">_________________________________</font></else>
			</cell>
		</table-row>
		<table-row>
			<cell></cell>
			<cell>
				<shr-label trim-body="true">Program Fax:<space/></shr-label>
				<if cond="${referralProgramFax!= null && referralProgramFax!= ''}"><shr-text>${referralProgramFax}</shr-text></if>
				<else><font name="times_8">_________________________________________</font></else>			
			</cell>
		</table-row>
		<table-row halign="left">
			<cell valign="top"></cell>
			<cell valign="top"><shr-label trim-body="true">Enrollment Number:<space/></shr-label>
				<if cond="${enrollmentNumber!= null && enrollmentNumber!= ""}"><font name="text">${enrollmentNumber}</font></if>
				<else><font name="times_8">______________________________________</font></else>
			</cell>
		</table-row>
	</table>	
		
	<paragraph halign="justified" spacing-before="5" line-spacing="1.5">
        <font name="times_8">
        	<shr-label trim-body="true">Client has been referred to your agency for the following reason(s): <br/></shr-label>
        	<if cond="${referralReasonComments!= null && referralReasonComments!= ""}"><shr-text>${referralReasonComments}</shr-text></if>
			<else><font name="times_8">__________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________________</font></else>
    	</font>
    </paragraph> 

</document>