		            	            <cell halign="left"><b>TDOEP</b></cell>
		                	        <cell halign="center"><b>Life Skills</b></cell>
		                    	    <cell halign="left"><b>SALCE</b></cell>
		                        	<cell halign="left"><b>DDAP</b></cell>
		                          	<cell halign="left"><b>WEST</b></cell>
		                          	<cell halign="left"><b></b></cell>
		                      	</table-row>
                      	 	</font>
							<font name="times_8">
			                    <table-row halign="left" valign="middle">
		    	                  	<cell halign="left">1<super>st</super> Enrollment</cell>  
		        	                <cell halign="left">$60.00</cell>
		            	            <cell halign="left">$60.00</cell>
		                	        <cell halign="center">$60.00 per course</cell>
		                    	    <cell halign="left">$15.00</cell>
		                        	<cell halign="left">$35.00</cell>
		                          	<cell halign="left">$35.00</cell>
		                          	<cell halign="left"></cell>
		                      	</table-row>
			                    <table-row halign="left" valign="middle">
		    	                  	<cell halign="left">2<super>nd</super> Enrollment</cell>  
		        	                <cell halign="left">$60.00</cell>
		            	            <cell halign="left">$60.00</cell>
		                	        <cell halign="center">$60.00 per course</cell>
		                    	    <cell halign="left"></cell>
		                        	<cell halign="left">$35.00</cell>
		                          	<cell halign="left">$35.00</cell>
		                          	<cell halign="left"></cell>
		                      	</table-row>
			                    <table-row halign="left" valign="middle">
		    	                  	<cell halign="left">3<super>rd</super> Enrollment</cell>  
		        	                <cell halign="left">$30.00</cell>
		            	            <cell halign="left">$40.00</cell>
		                	        <cell halign="center">$60.00 per course</cell>
		                    	    <cell halign="left"></cell>
		                        	<cell halign="left">$35.00</cell>
