				 	<if cond="${eachDate != null && eachDate != ""}"><u>${eachDate}</u></if><else>__________</else><br/>
					_endfor_</cell>
		   		<cell halign="left" border-style="none"  padding-bottom="10">
		   			_foreach_(prevTreatmentProgram ${prevTreatmentProgramsList} 'even' 'odd')
				 	<if cond="${prevTreatmentProgram != null && prevTreatmentProgram != ""}"><u>${prevTreatmentProgram}</u></if><else>____________________________________</else><br/>
					_endfor_
				</cell>
			 	<cell halign="left" border-style="none"  padding-bottom="10">
