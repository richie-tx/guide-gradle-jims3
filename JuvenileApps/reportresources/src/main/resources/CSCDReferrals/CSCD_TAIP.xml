			<cell halign="left" no-wrap="false" border-style="left|top">DEFENDANT</cell>
			<cell halign="left" border-style="top" no-wrap="true" >RACE</cell>
			<cell halign="left" border-style="top" no-wrap="true" >SEX</cell>
			<cell halign="left" border-style="top" no-wrap="true" >DOB</cell>
			<cell halign="left" border-style="top" no-wrap="true" >COURT</cell>
