						<cell></cell>
						<cell colspan="2" halign="left" border-style="none">PRIOR RECORD</cell>
					</table-row>
					<table-row>
						<cell halign="left" border-style="none">DATE</cell>
						<cell halign="left" border-style="none">OFFENSE</cell>
						<cell halign="left" border-style="none">DISPOSITION</cell>
					</table-row>
					_foreach_(eachCase ${priorOffenseRecordList} 'even' 'odd')
						<table-row halign="left" valign="top">
						   	<cell halign="left" border-style="none">
								<if cond="${eachCase.priorOffenseDate != null && eachCase.priorOffenseDate != ""}"><u>${eachCase.priorOffenseDate format "MM/dd/yyyy" }</u><br/></if><else>__________<br/></else>
						  	</cell>
							<cell halign="left" border-style="none">
								<if cond="${eachCase.offenseDesc != null && eachCase.offenseDesc != ""}"><u>${eachCase.offenseDesc}</u><br/></if><else>______________________<br/></else>
						  	</cell>
							<cell halign="left" border-style="none">
								<if cond="${eachCase.dispositionDesc!= null && eachCase.dispositionDesc!= ""}"><u>${eachCase.dispositionDesc}</u><br/></if><else>______________<br/></else>
							</cell>
						</table-row>
					_endfor_
		   		</table>
			</font>
		</cell>
		</table-row>
		<table-row>
			<cell colspan="2" halign="left" border-style="left|right|bottom" padding-bottom="10">
				<font name="arial">
		   		\tCURRENT OFFENSE: <if cond="${currentOffenseDate != null}"><u>${currentOffenseDate format "MM/dd/yyyy"}, </u></if><if cond="${currentOffense != null && currentOffense != ""}"><u>${currentOffense}</u></if><else>___________________</else><br/>
		   		</font>
		   	</cell>
		</table-row>
	</table>
	
   	<font size="8" style="normal">
		<table columns="4" widths="10, 35, 28, 27" border-style="none" keep-together="false" spacing-before="0" spacing-after="1" line-spacing="1.25" padding="2" >
