<?xml version="1.0" encoding="UTF-8"?>
<TestDataDictionary>
	<classification name="alpha">
		<generate type="String" sizeMin="1" sizeMax="20" />
	</classification>
	<classification name="int">
		<generate type="int" min="0" sizeMax="10000000" />
	</classification>
	<classification name="code">
		<generate type="text" size="3" />
	</classification>		
	<classification name="heightInches">
		<generate type="int" min="0" max="144" />
	</classification>
	<classification name="phoneNum">
		<generate type="int" min="1111111111" max="9999999999" />
	</classification>
	<classification name="ssn">
		<generate type="int" min="111111111" max="999999999" />
	</classification>	
	<classification name="currentDate">
		 <range type="Date" min="none" max="none" />
	</classification>
	<classification name="pastFiftyYears">		
		<generate type="Date" min="none" max="none" difference="-50" increment="year" />
	</classification>	
</TestDataDictionary>