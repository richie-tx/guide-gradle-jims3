<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">
          
<struts-config>


	<!-- Form Beans -->
	<form-beans>
		<form-bean name="codetableForm" type="ui.codetable.form.CodetableForm"/>
		<form-bean name="codetableRegistrationForm" type="ui.codetable.form.CodetableRegistrationForm"/>
	</form-beans>

	<!-- Global Exceptions -->
  	<global-exceptions>
		<exception key="exception.title" type="java.lang.Exception" handler="ui.exception.StrutsExceptionHandler" />
		<exception key="exception.title" type="javax.servlet.jsp.JspException" handler="ui.exception.StrutsExceptionHandler" />
		<exception key="exception.title" type="javax.servlet.error.exception" handler="ui.exception.StrutsExceptionHandler" />
		<exception key="exception.title" type="org.apache.struts.action.EXCEPTION" handler="ui.exception.StrutsExceptionHandler" />
		<exception key="exception.title" type="mojo.km.messaging.exception.ReturnException" handler="ui.exception.ReturnExceptionHandler" />
	</global-exceptions>

	<!-- Global Forwards -->
	<global-forwards>
		<forward name="handleException" path="/jsp/error.jsp" />
		<forward name="cancelMainPageHome" path="/jsp/cancel.htm" />
	</global-forwards>

	<!-- Action Mappings -->
	<action-mappings>
		<!-- GENERIC MAPPINGS -->		
		<action path="/globalBack" forward="/jsp/back.jsp"/>
		<action path="/globalCancel" forward="/jsp/cancel.htm"/>
		
		<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~ MANAGE CODE ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	    <action path="/displayCodetableRecordSearch" name="codetableForm" scope="session" type="ui.codetable.action.DisplayCodetableRecordSearchAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetable/codetableSearch.jsp"/>
		</action>		
		<action path="/displayCodetableRecordSearchResults" name="codetableForm" scope="session" type="ui.codetable.action.DisplayCodetableRecordSearchResultsAction" parameter="submitAction">
			<forward name="success" path="/displayCodetableDataList.do"/>
			<forward name="findSuccess" path="/jsp/codetable/codetableSearchResults.jsp"/>
			<forward name="searchFailure" path="/jsp/codetable/codetableSearch.jsp"/>
			<forward name="refreshSuccess" path="/jsp/codetable/codetableSearch.jsp"/>
			<forward name="cancel" path="/globalCancel.do"/>
		</action>
		<action path="/displayCodetableDataList" name="codetableForm" scope="session" type="ui.codetable.action.DisplayCodetableDataListAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetable/codetableDataList.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/jsp/codetable/codetableSearch.jsp"/>
			<forward name="failure" path="/jsp/codetable/codetableSearchResults.jsp"/>
		</action>
		<action path="/handleCodeSelection" name="codetableForm" scope="session" type="ui.codetable.action.HandleCodeSelectionAction" parameter="submitAction">
			<forward name="createSuccess" path="/jsp/codetable/codetableRecordUpdate.jsp"/>
			<forward name="viewSuccess" path="/jsp/codetable/codetableRecordDetails.jsp"/>
			<forward name="updateSuccess" path="/jsp/codetable/codetableRecordUpdate.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/jsp/codetable/codetableSearch.jsp"/>	
			<forward name="filterSuccess" path="/jsp/codetable/codetableDataList.jsp?pager.offset=0"/>
			<forward name="Reset" path="/jsp/codetable/codetableDataList.jsp"/>	
		</action>

		<action path="/displayUpdateCodetableConfirmation" name="codetableForm" scope="session" type="ui.codetable.action.DisplayUpdateCodetableConfirmationAction" parameter="submitAction">
			<forward name="updateSuccess" path="/jsp/codetable/codetableRecordDetails.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/jsp/codetable/codetableDataList.jsp"/>
			<forward name="createFailure" path="/jsp/codetable/codetableRecordUpdate.jsp"/>
		</action>
		<action path="/submitCodetableRecordUpdate" name="codetableForm" scope="session" type="ui.codetable.action.SubmitCodetableRecordUpdateAction" parameter="submitAction">
			<forward name="updateConfirmSuccess" path="/jsp/codetable/codetableRecordDetails.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/jsp/codetable/codetableDataList.jsp"/>
			<forward name="mainPage" path="/displayCodetableDataList.do?submitAction=Link"/>
			<forward name="validateFailure" path="/jsp/codetable/codetableRecordUpdate.jsp"/>
			<forward name="backToSearch" path="/jsp/codetable/codetableSearchResults.jsp"/>
		</action>
		<action path="/displayCodetableRegistrationSearch" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationSearchAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationSearch.jsp"/>
		</action>
		<action path="/displayCodetableRegistrationSearchResults" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationSearchResultsAction" parameter="submitAction">
			<forward name="success" path="/jsp/securityInquiries/securityInquiriesAgencyDetails.jsp"/>
			<forward name="findSuccess" path="/jsp/codetableRegistration/codeTableRegistrationSearchResults.jsp"/>
			<forward name="searchFailure" path="/jsp/codetableRegistration/codeTableRegistrationSearch.jsp"/>
			<forward name="refreshSuccess" path="/jsp/codetableRegistration/codeTableRegistrationSearch.jsp"/>
			<forward name="cancel" path="/globalCancel.do"/>
			<forward name="search" path="/handleCodetableRegistration.do"/>
		</action>
		<action path="/handleCodetableRegistration" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.HandleCodetableRegistrationAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="back" path="/globalBack.do"/>	
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>
		</action>
		<action path="/displayCodetableRegistrationUpdate" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationUpdateAction" parameter="submitAction">
			<forward name="create" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>	
			<forward name="update" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/globalCancel.do"/>
		</action>
		<action path="/displayCodeTableRegistrationSummary" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodeTableRegistrationSummaryAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="updateContinue" path="/jsp/codetableRegistration/codeTableRegistrationContextEntitySearchResults.jsp"/>
			<forward name="failure" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>	
			<forward name="resetSuccess" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>	
		</action>
		<action path="/handleCodeTableAgencyRemove" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.HandleCodetableAgencyRemoveAction" parameter="submitAction">
			<forward name="createRemoveAgencySuccess" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>
	    </action>
		<action path="/handleCodetableRegistrationEntity" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.HandleCodetableRegistrationEntityAction" parameter="submitAction">
			<forward name="back" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>
			<forward name="next" path="/displayCodeTableRegistrationSummary.do?submitAction=Submit"/>	
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>		
		</action>
		<action path="/submitCodeRegistrationUpdate" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.SubmitCodeRegistrationUpdateAction" parameter="submitAction">
			<forward name="finish" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>	
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>
			<forward name="back" path="/jsp/codetableRegistration/codeTableRegistrationUpdate.jsp"/>
		</action>
		<action path="/displayCodetableRegistrationAttribute" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationAttributeAction" parameter="submitAction">
			<forward name="failure" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationAttributeListUpdate.jsp"/>	
		</action>
		<action path="/displayCodetableRegistrationAttributesSummary" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationAttributesSummaryAction" parameter="submitAction">
			<forward name="validateFailure" path="/jsp/codetableRegistration/codeTableRegistrationAttributeListUpdate.jsp"/>
			<forward name="validateSuccess" path="/jsp/codetableRegistration/codeTableRegistrationAttributeListUpdate.jsp"/>	
			<forward name="updateSuccess" path="/jsp/codetableRegistration/codeTableRegistrationAttributeUpdate.jsp"/>
			<forward name="updateFailure" path="/jsp/codetableRegistration/codeTableRegistrationAttributeUpdate.jsp"/>
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="back" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>
		</action>
		<action path="/submitCodetableRegistrationAttributeUpdate" name="codetableRegistrationForm" scope="session" type="ui.codetable.action.SubmitCodetableRegistrationAttributeUpdateAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="failure" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="resequenceSuccess" path="/jsp/codetableRegistration/codeTableRegistrationSequenceAttributes.jsp"/>
			<forward name="back" path="/jsp/codetableRegistration/codeTableRegistrationAttributeListUpdate.jsp"/>
			<forward name="cancel" path="/displayCodetableRegistrationSearch.do?submitAction=Link"/>
		</action>
		<action path="/displayCodetableRegistrationTest" name="codetableForm" scope="session" type="ui.codetable.action.DisplayCodetableRegistrationTestAction" parameter="submitAction">
			<forward name="success" path="/jsp/codetable/codetableDataList.jsp"/>
			<forward name="validateFailure" path="/jsp/codetableRegistration/codeTableRegistrationDetails.jsp"/>
			<forward name="back" path="/globalBack.do"/>
			<forward name="cancel" path="/jsp/codetable/codetableSearch.jsp"/>
		</action>
	</action-mappings>

	<!-- Message Resources -->
	<message-resources parameter="ApplicationResources_CommonFunctionality"/>
	
	<!-- ========== Plug Ins Configuration ======================== -->
  <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
    <set-property
        property="pathnames"
        value="/WEB-INF/validator-rules.xml,
               /WEB-INF/validation.xml"/>
  </plug-in>

</struts-config>
